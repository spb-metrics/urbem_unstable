            <xml-property name="queryText"><![CDATA[    SELECT nom_municipio || ', ' || publico.fn_data_extenso(TO_DATE(NOW()::VARCHAR,'yyyy-mm-dd')) AS data_extenso
      FROM public.sw_municipio
     WHERE sw_municipio.cod_uf = ( SELECT valor
                                     FROM administracao.configuracao
                                    WHERE configuracao.exercicio  = ?
                                      AND configuracao.cod_modulo = 2
                                      AND configuracao.parametro  = 'cod_uf' )::INTEGER
       AND sw_municipio.cod_municipio = ( SELECT valor
                                          FROM administracao.configuracao
                                         WHERE configuracao.exercicio  = ?
                                           AND configuracao.cod_modulo = 2
                                           AND configuracao.parametro  = 'cod_municipio' )::INTEGER
]]></xml-property>
