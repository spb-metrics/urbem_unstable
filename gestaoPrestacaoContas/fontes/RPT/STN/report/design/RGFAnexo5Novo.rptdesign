		, positivo						numeric
		, negativo						numeric
		, saldo							numeric
		, a_pagar_exercicio				numeric
		, a_pagar_exercicio_anteriores	numeric
		, valor_consignacao_positivo	numeric
		, valor_consignacao_negativo	numeric
		, consignacoes 					numeric
		, caixa							numeric
   
)
