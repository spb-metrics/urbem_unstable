    <data-sets>
        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="rsRelatorioDesonerados" id="1399">
            <structure name="cachedMetaData"/>
            <method name="beforeOpen"><![CDATA[if (params["stExercico"] != ''){
	this.queryText += " AND grupo_credito.ano_exercicio = "+params['stExercico'];
}

if (params["inCodImovel"] != ''){
	this.queryText += " AND desonerado_imovel.inscricao_municipal = "+params['inCodImovel'];
}

if (params["inCGM"] != ''){
	this.queryText += " AND desonerado.numcgm = "+params['inCGM'];
}

if (params["inCodGrupo"] != ''){
	this.queryText += " AND grupo_credito.cod_grupo  = "+params['inCodGrupo'];
}
  

this.queryText += "  	GROUP BY    							\n";
this.queryText += "  	grupo_credito.ano_exercicio 			\n";                
this.queryText += "  ,  desonerado_imovel.inscricao_municipal   \n";  
this.queryText += "  ,  desonerado.numcgm						\n";  
this.queryText += "  ,  sw_cgm.nom_cgm							\n";
this.queryText += "  ,  grupo_credito.cod_grupo || '/' || grupo_credito.ano_exercicio || '-' || grupo_credito.descricao	\n";

       
this.queryText += "   ORDER BY desonerado.numcgm ASC";   
     
params['stDebug'] = this.queryText;]]></method>
            <property name="dataSource">postgres</property>
            <xml-property name="queryText"><![CDATA[	SELECT	grupo_credito.ano_exercicio, 
            desonerado_imovel.inscricao_municipal, 
            desonerado.numcgm, 
            sw_cgm.nom_cgm, 
            grupo_credito.cod_grupo || '/' || grupo_credito.ano_exercicio || '-' || grupo_credito.descricao as grupo_de_credito 
            
	FROM arrecadacao.desonerado 
     
	INNER JOIN  sw_cgm
			ON  sw_cgm.numcgm = desonerado.numcgm                                                                      

	INNER JOIN  arrecadacao.desonerado_imovel
			ON  desonerado_imovel.cod_desoneracao  = desonerado.cod_desoneracao
			AND desonerado_imovel.numcgm = desonerado.numcgm
			AND desonerado_imovel.ocorrencia = desonerado.ocorrencia

    INNER JOIN  arrecadacao.desoneracao
			ON  desoneracao.cod_desoneracao = desonerado.cod_desoneracao

	INNER JOIN  arrecadacao.credito_grupo
			ON  credito_grupo.cod_credito = desoneracao.cod_credito

	INNER JOIN  arrecadacao.grupo_credito
			ON  grupo_credito.cod_grupo = credito_grupo.cod_grupo
			
    WHERE 1=1
    ]]></xml-property>
        </oda-data-set>
