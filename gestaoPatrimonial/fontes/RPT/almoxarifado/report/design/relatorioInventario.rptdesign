            <xml-property name="queryText"><![CDATA[SELECT  inventario.exercicio
       ,inventario.cod_almoxarifado
       ,(SELECT nom_cgm FROM sw_cgm where numcgm = almoxarifado.cgm_responsavel) AS responsavel
       ,(SELECT nom_cgm FROM sw_cgm where numcgm = almoxarifado.cgm_almoxarifado) AS almoxarifado
       ,inventario.cod_inventario
       ,inventario.dt_inventario
       ,inventario.observacao
       ,inventario.processado
       ,catalogo_classificacao.cod_estrutural
       ,CASE 
             WHEN inventario.processado=true  THEN 'Processado'
             WHEN inventario.processado=false THEN 'Aberto'
        END as processado_str
       ,sum(quantidade) as quantidade
       ,(SELECT SUM(lancamento_material.quantidade) 
           FROM almoxarifado.lancamento_material 
     INNER JOIN almoxarifado.natureza_lancamento
             ON lancamento_material.exercicio_lancamento = natureza_lancamento.exercicio_lancamento
            AND lancamento_material.num_lancamento = natureza_lancamento.num_lancamento
            AND lancamento_material.cod_natureza = natureza_lancamento.cod_natureza
            AND lancamento_material.tipo_natureza = natureza_lancamento.tipo_natureza
          WHERE lancamento_material.cod_item = inventario_itens.cod_item 
            AND natureza_lancamento.timestamp < inventario.dt_inventario
        ) AS qtde_estoque
       ,catalogo_item.descricao_resumida
       ,catalogo_item.cod_item
       ,unidade_medida.nom_unidade
       ,(SELECT MIN(cod_estrutural) 
           FROM almoxarifado.catalogo_classificacao 
     INNER JOIN almoxarifado.catalogo_item
             ON catalogo_item.cod_catalogo = catalogo_classificacao.cod_catalogo
            AND catalogo_item.cod_classificacao = catalogo_classificacao.cod_classificacao
     INNER JOIN almoxarifado.inventario_itens inventario_itens_join
             ON catalogo_item.cod_item = inventario_itens_join.cod_item            
          WHERE inventario_itens_join.cod_almoxarifado = inventario_itens.cod_almoxarifado
            AND inventario_itens_join.cod_inventario = inventario_itens.cod_inventario
        )    AS classificacao_min
       ,(SELECT MAX(cod_estrutural) 
           FROM almoxarifado.catalogo_classificacao 
     INNER JOIN almoxarifado.catalogo_item
             ON catalogo_item.cod_catalogo = catalogo_classificacao.cod_catalogo
            AND catalogo_item.cod_classificacao = catalogo_classificacao.cod_classificacao
     INNER JOIN almoxarifado.inventario_itens inventario_itens_join
             ON catalogo_item.cod_item = inventario_itens_join.cod_item            
          WHERE inventario_itens_join.cod_almoxarifado = inventario_itens.cod_almoxarifado
            AND inventario_itens_join.cod_inventario = inventario_itens.cod_inventario
        )    AS classificacao_max
       
FROM almoxarifado.inventario_itens
INNER JOIN almoxarifado.inventario
        ON inventario.exercicio = inventario_itens.exercicio
       AND inventario.cod_almoxarifado = inventario_itens.cod_almoxarifado
       AND inventario.cod_inventario   = inventario_itens.cod_inventario
INNER JOIN almoxarifado.almoxarifado
        ON almoxarifado.cod_almoxarifado = inventario.cod_almoxarifado    
INNER JOIN almoxarifado.catalogo_item 
        ON catalogo_item.cod_item = inventario_itens.cod_item
INNER JOIN almoxarifado.catalogo_classificacao
        ON catalogo_classificacao.cod_catalogo = catalogo_item.cod_catalogo
       AND catalogo_classificacao.cod_classificacao = catalogo_item.cod_classificacao
INNER JOIN administracao.unidade_medida
        ON catalogo_item.cod_grandeza = unidade_medida.cod_grandeza
       AND catalogo_item.cod_unidade = unidade_medida.cod_unidade
WHERE 1=1
 

]]></xml-property>
