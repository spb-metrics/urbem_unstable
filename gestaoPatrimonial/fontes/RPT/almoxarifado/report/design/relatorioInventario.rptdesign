            <method name="beforeOpen"><![CDATA[var stFiltro="";
if(params["exercicio"] != "") {
	stFiltro = stFiltro + " AND inventario.exercicio = '" + params["exercicio_inventario"] + "'";
}
if(params["cod_almoxarifado"] != "") {
	stFiltro = stFiltro + " AND inventario.cod_almoxarifado = " + params["cod_almoxarifado"];
}
if(params["cod_inventario"] != "") {
	stFiltro = stFiltro + " AND inventario.cod_inventario = " + params["cod_inventario"];
}

stFiltro = stFiltro +" GROUP BY  inventario.exercicio, inventario_itens.cod_almoxarifado, inventario_itens.cod_inventario, inventario_itens.cod_item, inventario.cod_almoxarifado, almoxarifado.cgm_responsavel, almoxarifado.cgm_almoxarifado, inventario.cod_inventario, inventario.dt_inventario, inventario.observacao, inventario.processado,catalogo_classificacao.cod_estrutural, catalogo_item.cod_item, catalogo_item.descricao_resumida, unidade_medida.nom_unidade";

if(params["tipo_ordenacao"] != "") {
	stFiltro = stFiltro + " ORDER BY "+params["tipo_ordenacao"];
}

if(stFiltro != ""){
	this.queryText = this.queryText + stFiltro;
}

//params["sql"].value=this.queryText;]]></method>
