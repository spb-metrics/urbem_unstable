            <method name="onPrepare"><![CDATA[function retornaMesExtenso(mes)
{
	var arrayMes = new Array();
	arrayMes[0] = 'Janeiro';
	arrayMes[1] = 'Fevereiro';
	arrayMes[2] = 'Março';
	arrayMes[3] = 'Abril';
	arrayMes[4] = 'Maio';
	arrayMes[5] = 'Junho';
	arrayMes[6] = 'Julho';
	arrayMes[7] = 'Agosto';
	arrayMes[8] = 'Setembro';
	arrayMes[9] = 'Outubro';
	arrayMes[10] = 'Novembro';
	arrayMes[11] = 'Dezembro';
	
	return arrayMes[(mes-1)];
}

function retornaDataExtenso(data)
{
	data = data.split('/');
	var mes = retornaMesExtenso(data[1]);
	
	return data[0] + ' de ' + mes + ' de ' + data[2];
}]]></method>
