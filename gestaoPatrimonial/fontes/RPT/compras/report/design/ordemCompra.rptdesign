    <method name="initialize"><![CDATA[function float2moeda(num, ponto)
{
   x = 0;

   if(num<0) {
      num = Math.abs(num);
      x = 1;
   }

   if(isNaN(num)) num = "0";
      cents = Math.floor((num*100+0.5)%100);

   num = Math.floor((num*100+0.5)/100).toString();

   if(cents < 10) cents = "0" + cents;
      for (var i = 0; i < Math.floor((num.length-(1+i))/3); i++)
         num = num.substring(0,num.length-(4*i+3))+'.'
               +num.substring(num.length-(4*i+3));
   
   ret = num + (ponto ? '.' : ',') + cents;
   if (x == 1) ret = ' - ' + ret;

   return ret;

}


function formataNumExenteso(valor)
{
    var singular = new Array("centavo", "real", "mil", "milhão", "bilhão", "trilhão", "quatrilhão");
    var plural = new Array("centavos", "reais", "mil", "milhões", "bilhões", "trilhões", "quatrilhões");
    var c = new Array("", "cem", "duzentos", "trezentos", "quatrocentos", "quinhentos", "seiscentos", "setecentos", "oitocentos", "novecentos");
    var d = new Array("", "dez", "vinte", "trinta", "quarenta", "cinquenta", "sessenta", "setenta", "oitenta", "noventa");
    var d10 = new Array("dez", "onze", "doze", "treze", "quatorze", "quinze", "dezesseis", "dezesete", "dezoito", "dezenove");
    var u = new Array("", "um", "dois", "três", "quatro", "cinco", "seis", "sete", "oito", "nove");

    
    valor = float2moeda(valor, true);
    
    var inteiro = new Array();
    inteiro = valor.split(".");
    
    var i, j;
    for(i=0; i<inteiro.length; i++) {
        for(j=inteiro[i].length; j<3; j++) {
            inteiro[i] = "0"+inteiro[i];
        }
    }
    
    var fim = inteiro.length - 2;
    var rc, rd, ru, r, t;
    var rt = '';
    var z = 0;
    
    for(i=0; i<inteiro.length; i++) {
        valor = inteiro[i];
        rc = ( (valor > 100) &amp;&amp; (valor < 200) ) ? 'cento' : c[valor[0]];
        rd = (valor[1] < 2) ? '' : d[valor[1]];
        ru = (valor > 0) ? ((valor[1]) == 1 ? d10[valor[2]] : u[valor[2]]) : '';
        r = rc + ((rc &amp;&amp; (rd || ru)) ? " e " : '') + rd + ((rd &amp;&amp; ru) ? ' e ' : '') + ru;
        t = (((inteiro.length) - 1) - i)
        r += r ? ' '+(valor > 1 ? plural[t] : singular[t] ) : '';
        
        if (valor == '000') z++; else if (z > 0) z--;
        if ((t==1)&amp;&amp;(z>0)&amp;&amp;(inteiro[0]>0)) { r += ((z>1) ? ' de ' : '')+plural[t]; }
        if (r) rt = rt + (((i>0)&amp;&amp;(inteiro[0]>0)&amp;&amp;(z<1)) ? ( (i < fim) ? ', ' : ' e ') : ' ') + r;
    }

    return rt;
}]]></method>
