]]></method>
            <property name="dataSource">Postgres</property>
            <xml-property name="queryText"><![CDATA[ SELECT 
    solicitacao.cod_entidade,
    solicitacao_entidade.nom_cgm as entidade_nomcgm ,
    solicitacao.cod_solicitacao ,
    solicitacao.exercicio ,
    solicitacao.cod_almoxarifado as almoxarifado_codigo ,
    solicitacao_almoxarifado.nom_cgm as almoxarifado_nomcgm ,
    solicitante.numcgm as solicitante_numcgm ,
    solicitante.nom_cgm as solicitante_nomcgm ,
    requisitante.numcgm as requisitante_numcgm,
    requisitante.nom_cgm as requisitante_nomcgm ,
    objeto.cod_objeto as objeto_codigo ,
    REPLACE(objeto.descricao,'\\','') as objeto_descricao ,
    solicitacao.observacao ,
    prazo_entrega ,
    localizacao.numcgm as localizacao_numcgm,
    localizacao.nom_cgm as localizacao_nomcgm,
    solicitacao_convenio.exercicio_convenio,
    solicitacao_convenio.num_convenio,
    convenio.fundamentacao,
    (
        SELECT
           ( SUM(coalesce(solicitacao_item.vl_total, 0.00)) - SUM(coalesce(solicitacao_item_anulacao.vl_total, 0.00)) )  as vl_total
        FROM
            compras.solicitacao_item
            -- NOVO
            LEFT JOIN(
                       SELECT sum(coalesce(solicitacao_item_anulacao.vl_total   , 0.00   ))  as vl_total
                            , solicitacao_item_anulacao.exercicio
                            , solicitacao_item_anulacao.cod_entidade
                            , solicitacao_item_anulacao.cod_solicitacao
                            , solicitacao_item_anulacao.cod_centro
                            , solicitacao_item_anulacao.cod_item
                         FROM compras.solicitacao_item_anulacao
                     GROUP BY solicitacao_item_anulacao.exercicio
                            , solicitacao_item_anulacao.cod_entidade
                            , solicitacao_item_anulacao.cod_solicitacao
                            , solicitacao_item_anulacao.cod_centro
                            , solicitacao_item_anulacao.cod_item
                     ) as solicitacao_item_anulacao
                   ON(     solicitacao_item_anulacao.exercicio       = solicitacao_item.exercicio
                       AND solicitacao_item_anulacao.cod_entidade    = solicitacao_item.cod_entidade
                       AND solicitacao_item_anulacao.cod_solicitacao = solicitacao_item.cod_solicitacao
                       AND solicitacao_item_anulacao.cod_centro      = solicitacao_item.cod_centro
                       AND solicitacao_item_anulacao.cod_item        = solicitacao_item.cod_item  )

        WHERE
            solicitacao_item.cod_solicitacao = solicitacao.cod_solicitacao AND
            solicitacao_item.cod_entidade = solicitacao.cod_entidade AND
            solicitacao_item.exercicio = solicitacao.exercicio
    ) as vl_total_solicitacao,
    solicitacao.timestamp,
    TO_CHAR(solicitacao.timestamp,'dd/mm/yyyy') as data_solicitacao
FROM 
    compras.solicitacao 
JOIN
	orcamento.entidade
ON
	(
		solicitacao.cod_entidade = entidade.cod_entidade AND
		solicitacao.exercicio = entidade.exercicio
	)
JOIN
	sw_cgm as solicitacao_entidade
ON
	(entidade.numcgm = solicitacao_entidade.numcgm)
JOIN
	sw_cgm as solicitante
ON
	(solicitacao.cgm_solicitante = solicitante.numcgm)
JOIN
	sw_cgm as requisitante
ON
	(solicitacao.cgm_requisitante = requisitante.numcgm)
JOIN 
	almoxarifado.almoxarifado
ON
	(almoxarifado.cod_almoxarifado = solicitacao.cod_almoxarifado)
JOIN
	sw_cgm as solicitacao_almoxarifado
ON
	(almoxarifado.cgm_almoxarifado = solicitacao_almoxarifado.numcgm)
JOIN
	compras.solicitacao_entrega
ON 
	(
	solicitacao.exercicio = solicitacao_entrega.exercicio AND
	solicitacao.cod_entidade = solicitacao_entrega.cod_entidade AND
	solicitacao.cod_solicitacao = solicitacao_entrega.cod_solicitacao
	)
JOIN
	sw_cgm as localizacao
ON
	(solicitacao_entrega.numcgm = localizacao.numcgm)
JOIN
	compras.objeto
ON
	(solicitacao.cod_objeto = objeto.cod_objeto)
LEFT JOIN
    compras.solicitacao_convenio
    ON (solicitacao_convenio.exercicio       =  solicitacao.exercicio
   AND  solicitacao_convenio.cod_entidade    =  solicitacao.cod_entidade
   AND  solicitacao_convenio.cod_solicitacao =  solicitacao.cod_solicitacao)

LEFT JOIN
    licitacao.convenio
    ON (convenio.exercicio    = solicitacao_convenio.exercicio_convenio
   AND  convenio.num_convenio = solicitacao_convenio.num_convenio)	
WHERE
    solicitacao.cod_solicitacao = ?::integer AND
    solicitacao.cod_entidade = ?::integer
 ]]></xml-property>
