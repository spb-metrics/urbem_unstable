this.queryText=this.queryText+" ORDER BY codigo_item "]]></method>
            <property name="dataSource">Postgres</property>
            <xml-property name="queryText"><![CDATA[SELECT
    solicitacao_item.cod_item as codigo_item,
    catalogo_item.descricao as descricao_item,
    unidade_medida.nom_unidade as unidade,
    solicitacao_item.cod_centro as centro,
    solicitacao_item.complemento,
    solicitacao_item_dotacao.cod_despesa,
    despesa.cod_recurso as recurso,
    REPLACE (conta_despesa.descricao,'  ','') as descricao_desdob,
    despesa.num_orgao,
    despesa.num_unidade,
    despesa.cod_funcao,
    despesa.cod_subfuncao,
    despesa.cod_programa,
    ppa.programa.num_programa AS num_programa,
    ppa.acao.num_acao AS num_acao,
    despesa.num_pao,
    orcamento.orgao.nom_orgao,
    funcao.descricao as funcao,
    subfuncao.descricao as subfuncao,
    orcamento.programa.descricao as programa,
    pao.nom_pao,
    conta_despesa.cod_estrutural as clasificacao,
    solicitacao_item_dotacao.cod_conta ,
    recurso.nom_recurso,
    centro_custo.descricao as centrocusto,
    (  solicitacao_item.quantidade - solicitacao_item_anulacao.quantidade  ) as quantidade,
    (  solicitacao_item.vl_total   - solicitacao_item_anulacao.vl_total    ) as vl_total,
    -- ROUND (solicitacao_item_dotacao.quantidade,4) as quantidade_dotacao,
    coalesce (solicitacao_item_dotacao.quantidade, 0.0000) 
    - 
    coalesce (
                (select sum(solicitacao_item_dotacao_anulacao.quantidade)
                from compras.solicitacao_item_dotacao_anulacao
               where solicitacao_item_dotacao_anulacao.exercicio       = solicitacao_item_dotacao.exercicio                          
                 and solicitacao_item_dotacao_anulacao.cod_entidade    = solicitacao_item_dotacao.cod_entidade                       
                 and solicitacao_item_dotacao_anulacao.cod_solicitacao = solicitacao_item_dotacao.cod_solicitacao
                 and solicitacao_item_dotacao_anulacao.cod_item        = solicitacao_item_dotacao.cod_item
                 and solicitacao_item_dotacao_anulacao.cod_despesa     = solicitacao_item_dotacao.cod_despesa
                ), 0.0000) as quantidade_dotacao,
    
    coalesce (((solicitacao_item.vl_total/solicitacao_item.quantidade)*solicitacao_item_dotacao.quantidade), 0.0000) 
    - 
    coalesce (
                (select sum(solicitacao_item_dotacao_anulacao.vl_anulacao)
                from compras.solicitacao_item_dotacao_anulacao
               where solicitacao_item_dotacao_anulacao.exercicio       = solicitacao_item_dotacao.exercicio                          
                 and solicitacao_item_dotacao_anulacao.cod_entidade    = solicitacao_item_dotacao.cod_entidade                       
                 and solicitacao_item_dotacao_anulacao.cod_solicitacao = solicitacao_item_dotacao.cod_solicitacao
                 and solicitacao_item_dotacao_anulacao.cod_item        = solicitacao_item_dotacao.cod_item
                 and solicitacao_item_dotacao_anulacao.cod_despesa     = solicitacao_item_dotacao.cod_despesa
        ), 0.00) as vl_total_dotacao,

  --  ROUND (((solicitacao_item.vl_total/solicitacao_item.quantidade)*solicitacao_item_dotacao.quantidade),2) as vl_total_dotacao,
    (solicitacao_item.vl_total/solicitacao_item.quantidade)::numeric(14,2) as vl_unitario,

    ( SELECT REPLACE (conta_despesa.descricao,'  ','')
        FROM orcamento.conta_despesa
            WHERE cod_conta  = despesa.cod_conta
            AND   exercicio  = despesa.exercicio) as descricao,
    ( SELECT cod_estrutural
        FROM orcamento.conta_despesa
            WHERE cod_conta  = despesa.cod_conta
            AND   exercicio  = despesa.exercicio) as cod_estrutural        
FROM
    compras.solicitacao_item

            LEFT JOIN(
                       SELECT sum(coalesce(solicitacao_item_anulacao.vl_total   , 0.00   ))  as vl_total
                            , sum(coalesce(solicitacao_item_anulacao.quantidade , 0.0000 ))  as quantidade
                            , solicitacao_item_anulacao.exercicio
                            , solicitacao_item_anulacao.cod_entidade
                            , solicitacao_item_anulacao.cod_solicitacao
                            , solicitacao_item_anulacao.cod_centro
                            , solicitacao_item_anulacao.cod_item
                         FROM compras.solicitacao_item_anulacao
                     GROUP BY solicitacao_item_anulacao.exercicio
                            , solicitacao_item_anulacao.cod_entidade
                            , solicitacao_item_anulacao.cod_solicitacao
                            , solicitacao_item_anulacao.cod_centro
                            , solicitacao_item_anulacao.cod_item
                     ) as solicitacao_item_anulacao
                   ON(     solicitacao_item_anulacao.exercicio       = solicitacao_item.exercicio
                       AND solicitacao_item_anulacao.cod_entidade    = solicitacao_item.cod_entidade
                       AND solicitacao_item_anulacao.cod_solicitacao = solicitacao_item.cod_solicitacao
                       AND solicitacao_item_anulacao.cod_centro      = solicitacao_item.cod_centro
                       AND solicitacao_item_anulacao.cod_item        = solicitacao_item.cod_item  )

INNER JOIN
    almoxarifado.catalogo_item
ON
    (solicitacao_item.cod_item = catalogo_item.cod_item)
    
LEFT OUTER JOIN    compras.solicitacao_item_dotacao
ON  	    
	solicitacao_item.cod_solicitacao = solicitacao_item_dotacao.cod_solicitacao
AND  	solicitacao_item.exercicio = solicitacao_item_dotacao.exercicio
AND  	solicitacao_item.cod_entidade = solicitacao_item_dotacao.cod_entidade
AND  	solicitacao_item.cod_centro = solicitacao_item_dotacao.cod_centro
AND  	solicitacao_item.cod_item = solicitacao_item_dotacao.cod_item
    
INNER JOIN
    almoxarifado.centro_custo
ON
    (solicitacao_item.cod_centro = centro_custo.cod_centro)
INNER JOIN
    administracao.unidade_medida
ON
    (
    catalogo_item.cod_unidade = unidade_medida.cod_unidade and
    catalogo_item.cod_grandeza = unidade_medida.cod_grandeza
    )
LEFT JOIN  
	orcamento.despesa
	ON (solicitacao_item_dotacao.exercicio    = despesa.exercicio
    AND solicitacao_item_dotacao.cod_despesa  = despesa.cod_despesa
    AND solicitacao_item_dotacao.cod_entidade = despesa.cod_entidade
	)
JOIN orcamento.pao_ppa_acao
  ON pao_ppa_acao.num_pao = despesa.num_pao
 AND pao_ppa_acao.exercicio = despesa.exercicio
JOIN ppa.acao 
  ON ppa.acao.cod_acao = pao_ppa_acao.cod_acao
LEFT JOIN 
	orcamento.conta_despesa
	ON (conta_despesa.exercicio  = solicitacao_item_dotacao.exercicio
	AND conta_despesa.cod_conta  = solicitacao_item_dotacao.cod_conta
	)
LEFT JOIN  
	orcamento.recurso
	ON (despesa.exercicio   = recurso.exercicio
    AND despesa.cod_recurso = recurso.cod_recurso
	)
LEFT JOIN  
	orcamento.funcao
	ON (despesa.exercicio   = funcao.exercicio
    AND despesa.cod_funcao  = funcao.cod_funcao
	)
LEFT JOIN  
	orcamento.subfuncao
	ON (despesa.exercicio   = subfuncao.exercicio
    AND despesa.cod_subfuncao = subfuncao.cod_subfuncao
	)
LEFT JOIN  
	orcamento.programa
	ON (despesa.exercicio   = orcamento.programa.exercicio
    AND despesa.cod_programa = orcamento.programa.cod_programa
	)	
JOIN orcamento.programa_ppa_programa
  ON programa_ppa_programa.cod_programa = orcamento.programa.cod_programa
 AND programa_ppa_programa.exercicio    = orcamento.programa.exercicio
JOIN ppa.programa
  ON ppa.programa.cod_programa = orcamento.programa_ppa_programa.cod_programa_ppa
LEFT JOIN  
	orcamento.pao
	ON (despesa.exercicio = pao.exercicio
    AND despesa.num_pao   = pao.num_pao
	)
LEFT JOIN  
	orcamento.unidade
	ON (despesa.exercicio   = unidade.exercicio
    AND despesa.num_unidade = unidade.num_unidade
    AND despesa.num_orgao   = unidade.num_orgao
	)
LEFT JOIN  
	orcamento.orgao
	ON (unidade.exercicio   = orcamento.orgao.exercicio
    AND unidade.num_orgao   = orcamento.orgao.num_orgao
	)
WHERE
    solicitacao_item.cod_solicitacao = ? AND
    solicitacao_item.cod_entidade = ? 
    

]]></xml-property>
