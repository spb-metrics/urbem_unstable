	,	assinatura.cargo
	,	assinatura.timestamp
	,	sw_cgm.nom_cgm
FROM administracao.assinatura
INNER JOIN sw_cgm 
		ON assinatura.numcgm = sw_cgm.numcgm
WHERE assinatura.exercicio = ?::varchar]]></xml-property>
