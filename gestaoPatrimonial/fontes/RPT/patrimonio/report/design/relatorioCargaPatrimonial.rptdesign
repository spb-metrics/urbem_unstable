            <property name="dataSource">postgres</property>
            <xml-property name="queryText"><![CDATA[   Select natureza.nom_natureza
                     , natureza.cod_natureza
                     , grupo.nom_grupo
                     , grupo.cod_grupo
                     , especie.nom_especie
                     , especie.cod_especie
                     , vw_bem_ativo.cod_bem
                     , to_real(vw_bem_ativo.vl_bem) as vl_bem
                     , replace(vw_bem_ativo.descricao, chr(13), ' ') as descricao
                     , vw_bem_ativo.detalhamento
                     , to_char(vw_bem_ativo.dt_aquisicao,'dd/mm/yyyy') as dt_aquisicao
                     , situacao_bem.nom_situacao
                     , bem.num_placa
                     , bem_comprado.cod_empenho || '/' || bem_comprado.exercicio as num_empenho
                     , sw_cgm.nom_cgm as responsavel
                From patrimonio.natureza 
               
                join patrimonio.grupo 
                  on grupo.cod_natureza = natureza.cod_natureza
                
                join patrimonio.especie 
                  on grupo.cod_grupo = especie.cod_grupo
                 and natureza.cod_natureza = especie.cod_natureza
                
                join patrimonio.bem 
                  on bem.cod_natureza = especie.cod_natureza
                 And bem.cod_grupo = especie.cod_grupo
                 And bem.cod_especie = especie.cod_especie
                 
                join patrimonio.bem_responsavel
                  on bem_responsavel.cod_bem = bem.cod_bem
                 and bem_responsavel.timestamp = (select max(timestamp) 
                                                    from patrimonio.bem_responsavel br 
                                                   where br.cod_bem = bem.cod_bem)
                  
                  
                join sw_cgm 
                  on sw_cgm.numcgm = bem_responsavel.numcgm
                  
                join patrimonio.vw_bem_ativo 
                  on bem.cod_bem = vw_bem_ativo.cod_bem
                  
                join patrimonio.historico_bem 
                  on historico_bem.cod_bem = vw_bem_ativo.cod_bem  
                  
                join patrimonio.vw_ultimo_historico
                  on vw_ultimo_historico.cod_bem = historico_bem.cod_bem
                 And vw_ultimo_historico.timestamp = historico_bem.timestamp
                              
                join patrimonio.situacao_bem 
                  on historico_bem.cod_situacao = situacao_bem.cod_situacao

          left  join patrimonio.bem_comprado 
                  on bem_comprado.cod_bem = vw_bem_ativo.cod_bem
      
              where 1=1]]></xml-property>
        </oda-data-set>
