        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="RStermoResponsabilidade" id="1249">
            <list-property name="parameters">
                <structure>
                    <property name="name">param_numcgm</property>
                    <property name="paramName">numcgm</property>
                    <property name="dataType">string</property>
                    <property name="position">1</property>
                    <property name="isInput">true</property>
                    <property name="isOutput">false</property>
                </structure>
            </list-property>
            <structure name="cachedMetaData"/>
            <method name="beforeOpen"><![CDATA[if (params['lista_bens'] != ""){
    this.queryText += " AND bem.cod_bem in ("+params['lista_bens']+")";
}

this.queryText += " ORDER BY bem.descricao";

params["query"] = this.queryText;]]></method>
            <property name="dataSource">postgres</property>
            <xml-property name="queryText"><![CDATA[SELECT  bem.*
       ,bem_responsavel.*
       ,(SELECT nom_cgm 
          FROM sw_cgm 
         WHERE numcgm = bem_responsavel.numcgm) AS responsavel
         FROM  patrimonio.bem
   INNER JOIN  patrimonio.bem_responsavel
           ON  bem_responsavel.cod_bem = bem.cod_bem
         WHERE  bem_responsavel.numcgm  = ?
           AND  bem_responsavel.dt_fim IS NULL
		   AND NOT EXISTS
			    (
			        SELECT  1
			          FROM  patrimonio.bem_baixado
			         WHERE  bem_baixado.cod_bem = bem.cod_bem
			    )
]]></xml-property>
        </oda-data-set>
