            <method name="beforeOpen"><![CDATA[this.queryText  = " SELECT to_char(diaria.dt_inicio, 'dd/mm/yyyy')||' ('||to_char(diaria.hr_inicio, 'hh24:mi')||'h) a '||to_char(diaria.dt_termino,'dd/mm/yyyy')||' ('||to_char(diaria.hr_termino, 'hh24:mi')||'h)' as periodo                           \n";
this.queryText += "      , '' as dt_pagamento															   \n";
this.queryText += "      , diaria.quantidade															   \n";
this.queryText += "      , diaria.motivo                                                                   \n";
this.queryText += "      , diaria.vl_total as valor								                           \n";
this.queryText += "      , diaria.vl_unitario as valor_unitario								               \n";
this.queryText += "      , contrato.registro															   \n";
this.queryText += "      , sw_cgm.nom_cgm																   \n";
this.queryText += "      , norma.num_norma||'/'||norma.exercicio as ato_portaria						   \n";
this.queryText += "      , norma.dt_publicacao 															   \n";
this.queryText += "      , trim((select sem_acentos(nom_municipio)                                         \n";
this.queryText += "           from sw_municipio                                                            \n";
this.queryText += "          where cod_municipio = diaria.cod_municipio                                    \n";
this.queryText += "            and cod_uf = diaria.cod_uf))||'/'||trim((select sigla_uf from sw_uf where cod_uf = diaria.cod_uf)) as destino \n";

if(params["boAgrupar"] == ""){
	this.queryText += "  , '' as agrupamento \n";    
}

if(params["stTipoFiltro"] == "lotacao_grupo"){
	this.queryText += "      , recuperaDescricaoOrgao(contrato_servidor_orgao.cod_orgao,'"+params["exercicio"]+"-01-01'::date) as agrupamento  \n";
}
if(params["stTipoFiltro"] == "local_grupo"){
	this.queryText += "      , (SELECT descricao from organograma.local where cod_local = contrato_servidor_local.cod_local) as agrupamento  \n";
}
if(params["stTipoFiltro"] == "cargo_grupo"){
	this.queryText += "      , (SELECT descricao from pessoal.cargo where cod_cargo = contrato_servidor.cod_cargo) as agrupamento  \n";
}
if(params["stTipoFiltro"] == "funcao_grupo"){	
	this.queryText += "      , (SELECT descricao from pessoal.cargo where cod_cargo = contrato_servidor_funcao.cod_cargo) as agrupamento  \n";
}
if(params["stTipoFiltro"] == "sub_divisao_funcao_grupo"){	
	this.queryText += "      , (SELECT descricao from pessoal.regime where cod_regime = contrato_servidor_regime_funcao.cod_regime) ||' / '||  \n";
	this.queryText += "       (SELECT descricao from pessoal.sub_divisao where cod_sub_divisao = contrato_servidor_sub_divisao_funcao.cod_sub_divisao) as agrupamento  \n";
}

if(params["stTipoFiltro"] == "atributo_servidor_grupo"){	
	this.queryText += "  , (CASE WHEN atributo_dinamico.cod_tipo = 3 or atributo_dinamico.cod_tipo = 4              \n";
	this.queryText += "          THEN (SELECT valor_padrao                                                         \n";
	this.queryText += "       		     FROM administracao.atributo_valor_padrao                                  \n";
	this.queryText += "                 WHERE atributo_dinamico.cod_modulo = atributo_valor_padrao.cod_modulo      \n";
	this.queryText += "                   AND atributo_dinamico.cod_cadastro = atributo_valor_padrao.cod_cadastro  \n";
   this.queryText += "                   AND atributo_dinamico.cod_atributo = atributo_valor_padrao.cod_atributo) \n";
    this.queryText += "           ELSE atributo_contrato_servidor_valor.valor end) as agrupamento                        \n";
}

this.queryText += "  FROM diarias.diaria                            \n";
this.queryText += "     , (SELECT cod_diaria                        \n";
this.queryText += "             , cod_contrato                      \n";
this.queryText += "             , max(timestamp) as timestamp       \n";
this.queryText += "          FROM diarias.diaria                    \n";
this.queryText += "         GROUP BY cod_diaria                     \n";
this.queryText += "             , cod_contrato ) as max_diaria      \n";
this.queryText += "     , diarias.tipo_diaria                       \n";
this.queryText += "     , pessoal.contrato                          \n";
this.queryText += "     , pessoal.servidor_contrato_servidor        \n";
this.queryText += "     , pessoal.servidor                          \n";
this.queryText += "     , sw_cgm                                    \n";
this.queryText += "     , normas.norma                              \n";          
this.queryText += "     , pessoal.contrato_servidor                 \n";

if(params["stTipoFiltro"] == "lotacao_grupo"){       
	this.queryText += "      , pessoal.contrato_servidor_orgao                        \n";
	this.queryText += "      , (select cod_contrato                                   \n";
	this.queryText += "              , max(timestamp) as timestamp                    \n";
	this.queryText += "           from pessoal.contrato_servidor_orgao                \n";
	this.queryText += "         group by cod_contrato) as max_contrato_servidor_orgao \n";
}

if(params["stTipoFiltro"] == "local_grupo"){
	this.queryText += "      , pessoal.contrato_servidor_local                          \n";
	this.queryText += "      , (SELECT cod_contrato                                     \n";
	this.queryText += "              , max(timestamp) as timestamp                      \n";
	this.queryText += "           FROM pessoal.contrato_servidor_local                  \n";
	this.queryText += "          GROUP BY cod_contrato) as max_contrato_servidor_local  \n";
}

if(params["stTipoFiltro"] == "funcao_grupo"){	
	this.queryText += "      , pessoal.contrato_servidor_funcao                        \n";
	this.queryText += "      , (SELECT cod_contrato                                    \n";
	this.queryText += "              , max(timestamp) as timestamp                     \n";
	this.queryText += "           FROM pessoal.contrato_servidor_funcao                \n";
	this.queryText += "         GROUP BY cod_contrato) as max_contrato_servidor_funcao \n";
}        

if(params["stTipoFiltro"] == "sub_divisao_funcao_grupo"){	
	this.queryText += "      , pessoal.contrato_servidor_regime_funcao                         \n";
	this.queryText += "      , (SELECT cod_contrato                                            \n";
	this.queryText += "              , max(timestamp) as timestamp                             \n";
	this.queryText += "           FROM pessoal.contrato_servidor_regime_funcao                 \n";
	this.queryText += "         GROUP BY cod_contrato) as max_contrato_servidor_regime_funcao  \n";
	this.queryText += "      , pessoal.contrato_servidor_sub_divisao_funcao                    \n";
	this.queryText += "      , (SELECT cod_contrato                                            \n";
	this.queryText += "              , max(timestamp) as timestamp                             \n";
	this.queryText += "           FROM pessoal.contrato_servidor_sub_divisao_funcao            \n";
	this.queryText += "         GROUP BY cod_contrato) as max_contrato_servidor_sub_divisao_funcao \n";
}

if(params["stTipoFiltro"] == "atributo_servidor_grupo"){	
	this.queryText += "      , administracao.atributo_dinamico                                 \n";
	this.queryText += "      , pessoal.atributo_contrato_servidor_valor                        \n";
	this.queryText += "      , (SELECT cod_contrato                                            \n";
	this.queryText += "              , cod_atributo                                            \n";
	this.queryText += "              , max(timestamp) as timestamp                             \n";
	this.queryText += "           FROM pessoal.atributo_contrato_servidor_valor                \n";
	this.queryText += "         GROUP BY cod_contrato                                          \n";
	this.queryText += "                , cod_atributo) as max_atributo_contrato_servidor_valor \n";
}   

this.queryText += " WHERE diaria.cod_diaria     = max_diaria.cod_diaria                                    \n";
this.queryText += "   AND diaria.cod_contrato   = max_diaria.cod_contrato                                  \n";
this.queryText += "   AND diaria.timestamp      = max_diaria.timestamp                                     \n";
this.queryText += "   AND diaria.cod_norma      = norma.cod_norma                                          \n";
this.queryText += "   AND diaria.cod_contrato   = contrato.cod_contrato                                    \n";
this.queryText += "   AND diaria.cod_tipo       = tipo_diaria.cod_tipo                                     \n";
this.queryText += "   AND diaria.timestamp_tipo = tipo_diaria.timestamp									   \n";
this.queryText += "   AND contrato.cod_contrato = servidor_contrato_servidor.cod_contrato                  \n";
this.queryText += "   AND contrato.cod_contrato = contrato_servidor.cod_contrato                           \n";
this.queryText += "   AND servidor_contrato_servidor.cod_servidor = servidor.cod_servidor                  \n";
this.queryText += "   AND servidor.numcgm = sw_cgm.numcgm                                                  \n";



if(params["stTipoFiltro"] == "lotacao_grupo"){       
	this.queryText += "    AND contrato.cod_contrato = contrato_servidor_orgao.cod_contrato                            \n";
	this.queryText += "    AND contrato_servidor_orgao.cod_contrato = max_contrato_servidor_orgao.cod_contrato         \n";
	this.queryText += "    AND contrato_servidor_orgao.timestamp = max_contrato_servidor_orgao.timestamp               \n";
	this.queryText += "    AND contrato_servidor_orgao.cod_orgao IN ("+params["stCodigos"]+")                          \n";
}

if(params["stTipoFiltro"] == "funcao_grupo"){   
	this.queryText += "    AND contrato.cod_contrato = contrato_servidor_funcao.cod_contrato                          \n";
	this.queryText += "    AND contrato_servidor_funcao.cod_contrato = max_contrato_servidor_funcao.cod_contrato      \n";
	this.queryText += "    AND contrato_servidor_funcao.timestamp = max_contrato_servidor_funcao.timestamp            \n";
	this.queryText += "    AND contrato_servidor_funcao.cod_cargo IN ("+params["stCodigos"]+")                      \n";
}

if(params["stTipoFiltro"] == "sub_divisao_funcao_grupo") {
	this.queryText += "    AND contrato.cod_contrato = contrato_servidor_regime_funcao.cod_contrato                                         \n";
	this.queryText += "    AND contrato_servidor_regime_funcao.cod_contrato = max_contrato_servidor_regime_funcao.cod_contrato              \n";
	this.queryText += "    AND contrato_servidor_regime_funcao.timestamp = max_contrato_servidor_regime_funcao.timestamp                    \n";
	this.queryText += "    AND contrato.cod_contrato = contrato_servidor_sub_divisao_funcao.cod_contrato                                    \n";
	this.queryText += "    AND contrato_servidor_sub_divisao_funcao.cod_contrato = max_contrato_servidor_sub_divisao_funcao.cod_contrato    \n";
	this.queryText += "    AND contrato_servidor_sub_divisao_funcao.timestamp = max_contrato_servidor_sub_divisao_funcao.timestamp          \n";
	this.queryText += "    AND contrato_servidor_sub_divisao_funcao.cod_sub_divisao IN ("+params["stCodigos"]+")                            \n";
}

if(params["stTipoFiltro"] == "local_grupo"){
	this.queryText += "    AND contrato_servidor.cod_contrato = contrato_servidor_local.cod_contrato                  \n";
	this.queryText += "    AND contrato_servidor_local.cod_contrato = max_contrato_servidor_local.cod_contrato        \n";
	this.queryText += "    AND contrato_servidor_local.timestamp = max_contrato_servidor_local.timestamp              \n";
    this.queryText += "    AND contrato_servidor_local.cod_local IN ("+params["stCodigos"]+")                         \n";
}

if(params["stTipoFiltro"] == "atributo_servidor_grupo"){   
	this.queryText += "    AND contrato.cod_contrato = atributo_contrato_servidor_valor.cod_contrato                              \n";
	this.queryText += "    AND atributo_contrato_servidor_valor.cod_contrato = max_atributo_contrato_servidor_valor.cod_contrato  \n";
	this.queryText += "    AND atributo_contrato_servidor_valor.cod_atributo = max_atributo_contrato_servidor_valor.cod_atributo  \n";
	this.queryText += "    AND atributo_contrato_servidor_valor.timestamp = max_atributo_contrato_servidor_valor.timestamp        \n";
	this.queryText += "	   AND atributo_contrato_servidor_valor.cod_modulo = atributo_dinamico.cod_modulo                         \n";
	this.queryText += "    AND atributo_contrato_servidor_valor.cod_cadastro = atributo_dinamico.cod_cadastro                     \n";
	this.queryText += "	   AND atributo_contrato_servidor_valor.cod_atributo = atributo_dinamico.cod_atributo                     \n";
	this.queryText += "    AND atributo_contrato_servidor_valor.cod_atributo = "+params["inCodAtributo"]+"                        \n";		
	if (params["boArray"] == "true") {
		this.queryText += "    AND atributo_contrato_servidor_valor.valor IN ("+params["stCodigos"]+")                                \n";
	} else {
		this.queryText += "    AND atributo_contrato_servidor_valor.valor = '"+params["stCodigos"]+"'                                 \n";
	}
}

if(params["stTipoFiltro"] == "contrato" || params["stTipoFiltro"] == "cgm_contrato"){   
	this.queryText += "    AND contrato.cod_contrato IN ("+params["stCodigos"]+") \n";
