            <method name="beforeOpen"><![CDATA[if(params["inCodConfiguracao"] == "0"){
		this.queryText += "\n";
		this.queryText +=   "LEFT JOIN (SELECT fgts_categoria.cod_categoria\n";
		this.queryText +=   "	             , fgts_categoria.aliquota_deposito\n";
		this.queryText +=   "	             , fgts_categoria.aliquota_contribuicao\n";
		this.queryText +=   "	             , registro_evento_complementar.cod_contrato\n";
		this.queryText +=   "				 , sum(evento_complementar_calculado.valor) as valor\n";
		this.queryText +=   "		      FROM folhapagamento.fgts_evento\n";
		this.queryText +=   "				 , (SELECT cod_tipo\n";
		this.queryText +=   "				         , cod_fgts\n";
		this.queryText +=   "				         , max(timestamp) as timestamp\n";
		this.queryText +=   "		             FROM folhapagamento.fgts_evento\n";
		this.queryText +=   "			         GROUP by cod_tipo\n";
		this.queryText +=   "	   			            , cod_fgts) as max_fgts_evento\n";
		this.queryText +=   "                , folhapagamento.fgts_categoria\n";
		this.queryText +=   "                , folhapagamento.evento_complementar_calculado\n";
		this.queryText +=   "                , folhapagamento.registro_evento_complementar\n";
		this.queryText +=   "		     WHERE fgts_evento.cod_tipo = max_fgts_evento.cod_tipo\n";
		this.queryText +=   "			   AND fgts_evento.cod_fgts = max_fgts_evento.cod_fgts\n";
		this.queryText +=   "			   AND fgts_evento.timestamp = max_fgts_evento.timestamp\n";
		this.queryText +=   "			   AND fgts_evento.cod_fgts = fgts_categoria.cod_fgts\n";
		this.queryText +=   "			   AND fgts_evento.timestamp = fgts_categoria.timestamp\n";
		this.queryText +=   "			   AND fgts_evento.cod_evento = evento_complementar_calculado.cod_evento\n";
		this.queryText +=   "			   AND evento_complementar_calculado.cod_registro = registro_evento_complementar.cod_registro\n";
		this.queryText +=   "			   AND evento_complementar_calculado.cod_evento = registro_evento_complementar.cod_evento\n";
		this.queryText +=   "			   AND evento_complementar_calculado.timestamp_registro = registro_evento_complementar.timestamp\n";
		this.queryText +=   "			   AND evento_complementar_calculado.cod_configuracao = registro_evento_complementar.cod_configuracao\n";
		this.queryText +=   "			   AND registro_evento_complementar.cod_periodo_movimentacao = "+params["periodoMovimentacao"]+"\n";
		this.queryText +=   "			   AND registro_evento_complementar.cod_complementar = "+params["inCodComplementar"]+"\n";		
		this.queryText +=   "			   AND fgts_evento.cod_tipo = 3\n";
		this.queryText +=   "			   AND (desdobramento IS NULL OR desdobramento = 'A' OR desdobramento = 'F')\n";								
		this.queryText +=   "         GROUP BY fgts_categoria.cod_categoria\n";
		this.queryText +=   "	             , fgts_categoria.aliquota_deposito\n";
		this.queryText +=   "	             , fgts_categoria.aliquota_contribuicao\n";
		this.queryText +=   "	             , registro_evento_complementar.cod_contrato) as base_fgts\n";		
		this.queryText +=   "			    ON base_fgts.cod_categoria = contrato_servidor.cod_categoria\n";
		this.queryText +=   "			   AND base_fgts.cod_contrato = contrato_servidor.cod_contrato    \n";    
		this.queryText +=   "LEFT JOIN (SELECT fgts_categoria.cod_categoria\n";
		this.queryText +=   "                , registro_evento_complementar.cod_contrato\n";
		this.queryText +=   "                , SUM(evento_complementar_calculado.valor) as valor\n";
		this.queryText +=   "             FROM folhapagamento.fgts_evento\n";
		this.queryText +=   "                , (SELECT cod_tipo\n";
		this.queryText +=   "                        , cod_fgts\n";
		this.queryText +=   "                        , max(timestamp) as timestamp\n";
