            <xml-property name="queryText"><![CDATA[   SELECT contrato.registro
   	    , (SELECT nom_cgm FROM sw_cgm WHERE numcgm = servidor.numcgm) as nom_cgm
        , lpad(contrato_servidor.cod_categoria::varchar,2,'0') as cod_categoria
        , coalesce(base_fgts.valor,0) as base_fgts
        , 0.5 as aliquota_deposito
        , coalesce(base_fgts.aliquota_contribuicao,0) as aliquota_contribuicao
        , coalesce(recolhido_fgts.valor,0) as recolhido_fgts
        , coalesce(contribuicao_fgts.valor,0) as contribuicao_fgts
        , orgao.orgao
        , recuperaDescricaoOrgao(orgao.cod_orgao,(?||'-01-01')::date) as desc_orgao
   	    , (SELECT servidor_pis_pasep FROM sw_cgm_pessoa_fisica WHERE numcgm = servidor.numcgm) as servidor_pis_pasep        
        , to_char(contrato_servidor.dt_opcao_fgts,'dd/mm/yyyy') as dt_opcao_fgts
        , desc_local
     FROM pessoal.servidor
LEFT JOIN (SELECT servidor_pis_pasep.*
             FROM pessoal.servidor_pis_pasep
                , (SELECT cod_servidor
                        , max(timestamp) as timestamp
                     FROM pessoal.servidor_pis_pasep
                   GROUP BY cod_servidor) as max_servidor_pis_pasep
            WHERE servidor_pis_pasep.cod_servidor = max_servidor_pis_pasep.cod_servidor
              AND servidor_pis_pasep.timestamp = max_servidor_pis_pasep.timestamp) as servidor_pis_pasep
       ON servidor.cod_servidor = servidor_pis_pasep.cod_servidor    
        , pessoal.servidor_contrato_servidor
        , pessoal.contrato 
INNER JOIN (SELECT contrato_servidor_orgao.*
                , vw_orgao_nivel.orgao
             FROM ultimo_contrato_servidor_orgao(?,?) as contrato_servidor_orgao
       INNER JOIN organograma.vw_orgao_nivel
               ON contrato_servidor_orgao.cod_orgao = vw_orgao_nivel.cod_orgao) as orgao
       ON orgao.cod_contrato = contrato.cod_contrato       
LEFT JOIN (SELECT contrato_servidor_local.cod_local
                , contrato_servidor_local.cod_contrato       
                , local.descricao as desc_local 
             FROM ultimo_contrato_servidor_local(?,?) as contrato_servidor_local
       INNER JOIN organograma.local
               ON contrato_servidor_local.cod_local = local.cod_local) as local
       ON local.cod_contrato = contrato.cod_contrato
        , pessoal.contrato_servidor]]></xml-property>
