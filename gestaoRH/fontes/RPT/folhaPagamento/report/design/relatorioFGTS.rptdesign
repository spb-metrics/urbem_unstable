		this.queryText +=   "                          , cod_fgts) as max_fgts_evento\n";
		this.queryText +=   "                , folhapagamento.fgts_categoria\n";
		this.queryText +=   "                , folhapagamento.evento_complementar_calculado\n";
		this.queryText +=   "                , folhapagamento.registro_evento_complementar\n";
		this.queryText +=   "            WHERE fgts_evento.cod_tipo = max_fgts_evento.cod_tipo\n";
