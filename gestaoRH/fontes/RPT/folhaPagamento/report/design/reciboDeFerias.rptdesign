        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="consultaPrincipal" id="1150">
            <structure name="cachedMetaData"/>
            <method name="beforeOpen"><![CDATA[this.queryText  = "\n       SELECT contrato.* ";
this.queryText += "\n            , (SELECT nom_cgm FROM sw_cgm WHERE sw_cgm.numcgm = servidor.numcgm) as nom_cgm ";
this.queryText += "\n            , (SELECT descricao FROM pessoal.especialidade WHERE especialidade.cod_especialidade = contrato_servidor_especialidade_funcao.cod_especialidade_funcao) as desc_especialidade ";                         
this.queryText += "\n            , recuperaDescricaoOrgao(contrato_servidor_orgao.cod_orgao, to_date('"+params["dtFinalCompetencia"]+"', 'dd/mm/yyyy')) as desc_orgao ";                                                                    
this.queryText += "\n            , (SELECT descricao FROM pessoal.cargo WHERE cargo.cod_cargo = contrato_servidor_funcao.cod_cargo) as desc_funcao ";                                                                      
this.queryText += "\n            , (SELECT descricao FROM organograma.local WHERE cod_local = contrato_servidor_local.cod_local) as desc_local ";
this.queryText += "\n            , to_real(contrato_servidor_salario.salario) as salario ";
this.queryText += "\n            , ferias.faltas ";                                                                                                                                                                  
this.queryText += "\n            , ferias.dias_ferias ";                                                                                                                                                             
this.queryText += "\n            , ferias.dias_abono ";                                                                                                                                                              
this.queryText += "\n            , to_char(ferias.dt_inicial_aquisitivo, 'dd/mm/yyyy') as dt_inicial_aquisitivo ";                                                                                                    
this.queryText += "\n            , to_char(ferias.dt_final_aquisitivo, 'dd/mm/yyyy') as dt_final_aquisitivo ";                                                                                                      
this.queryText += "\n            , to_char(lancamento_ferias.dt_inicio, 'dd/mm/yyyy') as dt_inicio ";                                                                                                     
this.queryText += "\n            , to_char(lancamento_ferias.dt_fim, 'dd/mm/yyyy') as dt_fim ";                                                                                                        
this.queryText += "\n            , lancamento_ferias.mes_competencia ";                                                                                                                                              
this.queryText += "\n            , lancamento_ferias.ano_competencia ";                                                                                                                                               
this.queryText += "\n            , lancamento_ferias.cod_tipo ";
this.queryText += "\n            , vw_orgao_nivel.orgao as cod_estrutural ";
this.queryText += "\n         FROM pessoal.ferias ";
this.queryText += "\n   INNER JOIN pessoal.lancamento_ferias ";
this.queryText += "\n           ON ferias.cod_ferias = lancamento_ferias.cod_ferias ";
this.queryText += "\n   INNER JOIN pessoal.contrato ";
this.queryText += "\n           ON ferias.cod_contrato = contrato.cod_contrato ";
this.queryText += "\n   INNER JOIN pessoal.servidor_contrato_servidor "; 
this.queryText += "\n           ON contrato.cod_contrato = servidor_contrato_servidor.cod_contrato ";
this.queryText += "\n   INNER JOIN pessoal.servidor "; 
this.queryText += "\n           ON servidor_contrato_servidor.cod_servidor = servidor.cod_servidor ";
this.queryText += "\n   INNER JOIN ultimo_contrato_servidor_salario('"+params["stEntidade"]+"', "+params["inCodPeriodoMovimentacao"]+") as contrato_servidor_salario ";
this.queryText += "\n           ON contrato_servidor_salario.cod_contrato = contrato.cod_contrato ";
this.queryText += "\n   INNER JOIN ultimo_contrato_servidor_orgao('"+params["stEntidade"]+"', "+params["inCodPeriodoMovimentacao"]+") as contrato_servidor_orgao ";
this.queryText += "\n           ON contrato_servidor_orgao.cod_contrato = contrato.cod_contrato ";
this.queryText += "\n   INNER JOIN organograma.vw_orgao_nivel ";
this.queryText += "\n           ON vw_orgao_nivel.cod_orgao = contrato_servidor_orgao.cod_orgao ";
this.queryText += "\n   INNER JOIN ultimo_contrato_servidor_funcao('"+params["stEntidade"]+"', "+params["inCodPeriodoMovimentacao"]+") as contrato_servidor_funcao ";
this.queryText += "\n           ON contrato_servidor_funcao.cod_contrato = contrato.cod_contrato ";
this.queryText += "\n    LEFT JOIN ultimo_contrato_servidor_local('"+params["stEntidade"]+"', "+params["inCodPeriodoMovimentacao"]+") as contrato_servidor_local ";
this.queryText += "\n           ON contrato_servidor_local.cod_contrato = contrato.cod_contrato ";
this.queryText += "\n    LEFT JOIN ultimo_contrato_servidor_especialidade_funcao('"+params["stEntidade"]+"', "+params["inCodPeriodoMovimentacao"]+") as contrato_servidor_especialidade_funcao ";
this.queryText += "\n           ON contrato_servidor_especialidade_funcao.cod_contrato = contrato.cod_contrato ";
this.queryText += "\n        WHERE recuperarSituacaoDoContrato(contrato.cod_contrato,"+params["inCodPeriodoMovimentacao"]+",'"+params["stEntidade"]+"') != 'R'";
this.queryText += "\n	       AND lancamento_ferias.mes_competencia = '"+params["stMesCompetencia"]+"'";
this.queryText += "\n	 	   AND lancamento_ferias.ano_competencia = '"+params["stAnoCompetencia"]+"'";

// Adicionando os filtros
if( params["stTipoFiltro"] == "local" ){
	this.queryText += "\n  AND contrato_servidor_local.cod_local IN ("+params["stValor"]+")";
}

if( params["stTipoFiltro"] == "lotacao" ){
	this.queryText += "\n  AND contrato_servidor_orgao.cod_orgao IN ("+params["stValor"]+")";
}

if( params["stTipoFiltro"] == "cgm_contrato" || params["stTipoFiltro"] == "contrato" ){
	this.queryText += "\n  AND contrato.cod_contrato IN ("+params["stValor"]+")";
}

var ordenacao = new Array();
var contador = 0;
// Adicionando a ordenação
if( params["boOrdenacaoLotacao"] == "true" ){
    if( params["stOrdenacaoLotacao"] == "A" ){
        ordenacao[contador] = "desc_orgao";
    }else{
        ordenacao[contador] = "orgao";
    }
    contador++;
}

if( params["boOrdenacaoLocal"] == "true" ){
    if( params["stOrdenacaoLocal"] == "A" ){
        ordenacao[contador] = "desc_local";
    }else{
        ordenacao[contador] = "cod_local";
    }    
    contador++;
}

if( params["boOrdenacaoCGM"] == "true" ){
    if( params["stOrdenacaoCGM"] == "A" ){
        ordenacao[contador] = "nom_cgm";
    }else{
        ordenacao[contador] = "numcgm";
    }    
    contador++;
}

if(ordenacao.length > 0) {
	this.queryText += " ORDER BY "+ordenacao.join(',');
}

this.queryText = this.queryText.replaceAll("pessoal", "pessoal" + params["stEntidade"]);
this.queryText = this.queryText.replaceAll("folhapagamento", "folhapagamento" + params["stEntidade"]);
params["query"].value = this.queryText;]]></method>
