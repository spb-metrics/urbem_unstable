            <method name="beforeOpen"><![CDATA[this.queryText  = "\n SELECT * ";
this.queryText += "\n      , to_real("+params["nuTotalLiquido"]+") as liquido ";
this.queryText += "\n      , lower(publico.fn_extenso("+params["nuTotalLiquido"]+")) as liquido_extenso ";
this.queryText += "\n   FROM ( ";
this.queryText += "\n           SELECT sw_cgm.nom_cgm as nom_prefeitura ";
this.queryText += "\n                 , trim(coalesce(sw_cgm.tipo_logradouro,'')||' '||sw_cgm.logradouro) as logradouro ";
this.queryText += "\n                 , sw_cgm.numero as numero ";
this.queryText += "\n                 , entidade.cod_entidade ";
this.queryText += "\n                 , sw_municipio.nom_municipio as num_municipio ";
this.queryText += "\n             FROM orcamento.entidade ";
this.queryText += "\n       INNER JOIN sw_cgm ";
this.queryText += "\n               ON entidade.numcgm = sw_cgm.numcgm ";
this.queryText += "\n       INNER JOIN sw_municipio ";
this.queryText += "\n               ON sw_cgm.cod_municipio = sw_municipio.cod_municipio ";
this.queryText += "\n              AND sw_cgm.cod_uf = sw_municipio.cod_uf ";
this.queryText += "\n            WHERE entidade.exercicio = '"+params["exercicio"]+"' ";
this.queryText += "\n              AND entidade.cod_entidade = "+params["entidade"]+" ";
this.queryText += "\n       ) as temp_table ";]]></method>
