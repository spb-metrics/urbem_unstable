            <xml-property name="queryText"><![CDATA[  SELECT descricao
       , natureza
       , codigo
       , ( CASE WHEN natureza = 'D'
       			THEN 1
       			ELSE 0
          END) as ordem
       , quantidade
       , desdobramento_texto as desdobramento
       , (CASE WHEN natureza = 'D' 
       			  THEN valor
       			  ELSE 0.00
          END) AS desconto
       , (CASE WHEN natureza = 'P' 
       			  THEN valor
       			  ELSE 0.00
          END) AS provento         
    FROM recuperarEventosCalculados(?,?,?,?,?,?)
   WHERE natureza IN ('D', 'P')
     AND desdobramento != ''
ORDER BY 4,3;]]></xml-property>
