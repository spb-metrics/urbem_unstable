            <method name="beforeOpen"><![CDATA[if(params["inCodConfiguracao"] == 0){
        this.queryText   = "select evento.codigo \n";
        this.queryText  += "             , evento.descricao \n";
        this.queryText  += "             , evento.natureza \n";
        this.queryText  += "             , evento.tipo     \n";
        this.queryText  += "             , false as proporcional     \n";
        this.queryText  += "             , case registro_evento_complementar.cod_configuracao \n";
        this.queryText  += "               when 1 then 'Salário' \n";                
        this.queryText  += "               when 2 then 'Férias' \n";                
        this.queryText  += "               when 3 then '13º Salário' \n";                
        this.queryText  += "               when 4 then 'Rescisão' \n";                                        
        this.queryText  += "               end as desdobramento \n";        
        this.queryText  += "             , coalesce(registro_evento_complementar.quantidade,0.00) as quantidade \n";
        this.queryText  += "             , case when evento.natureza = 'P' then \n";
        this.queryText  += "                  coalesce(registro_evento_complementar.valor,0.00) \n";
        this.queryText  += "               else 0.00 end as provento \n";
        this.queryText  += "             , case when evento.natureza = 'D' then \n";
        this.queryText  += "                  coalesce(registro_evento_complementar.valor,0.00) \n";
        this.queryText  += "               else 0.00 end as desconto     \n";
        this.queryText  += "             , case when registro_evento_complementar_parcela.parcela is not null then \n";
        this.queryText  += "                    to_char(adiciona_meses(current_date,registro_evento_complementar_parcela.parcela),'mm/yyyy') \n";
        this.queryText  += "               end as ano_limite \n";
        this.queryText  += "          from folhapagamento.registro_evento_complementar \n";
        this.queryText  += "             , folhapagamento.ultimo_registro_evento_complementar \n";
        this.queryText  += "        left join folhapagamento.registro_evento_complementar_parcela \n";
        this.queryText  += "               on ultimo_registro_evento_complementar.cod_registro = registro_evento_complementar_parcela.cod_registro \n";
        this.queryText  += "              and ultimo_registro_evento_complementar.cod_evento = registro_evento_complementar_parcela.cod_evento\n";
        this.queryText  += "              and ultimo_registro_evento_complementar.cod_configuracao = registro_evento_complementar_parcela.cod_configuracao\n";
        this.queryText  += "              and ultimo_registro_evento_complementar.timestamp = registro_evento_complementar_parcela.timestamp\n";
        this.queryText  += "             , folhapagamento.evento\n";
        this.queryText  += "         where registro_evento_complementar.cod_registro = ultimo_registro_evento_complementar.cod_registro\n";
        this.queryText  += "           and registro_evento_complementar.cod_evento = ultimo_registro_evento_complementar.cod_evento\n";
        this.queryText  += "           and registro_evento_complementar.cod_configuracao = ultimo_registro_evento_complementar.cod_configuracao\n";
        this.queryText  += "           and registro_evento_complementar.timestamp = ultimo_registro_evento_complementar.timestamp\n";
        this.queryText  += "           and registro_evento_complementar.cod_evento = evento.cod_evento\n";
        this.queryText  += "           and registro_evento_complementar.cod_complementar = "+params["inCodComplementar"]+"\n";
        this.queryText  += "           and registro_evento_complementar.cod_periodo_movimentacao = ?\n";
        this.queryText  += "           and registro_evento_complementar.cod_contrato = ?"    
}
if(params["inCodConfiguracao"] == 1){
	this.queryText = "select evento.codigo\
     , evento.descricao\
     , evento.natureza\
     , evento.tipo     \
     , registro_evento.proporcional     \
     , coalesce(registro_evento.quantidade,0.00) as quantidade\
     , case when evento.natureza = 'P' then\
          coalesce(registro_evento.valor,0.00)\
       else 0.00 end as provento\
     , case when evento.natureza = 'D' then\
          coalesce(registro_evento.valor,0.00)\
       else 0.00 end as desconto     \
     , case when registro_evento_parcela.parcela is not null then\
            to_char(adiciona_meses(current_date,registro_evento_parcela.parcela),'mm/yyyy')\
       end as ano_limite\
     , (select desdobramento from folhapagamento.evento_calculado where cod_registro = registro_evento.cod_registro) as desdobramento\
  from folhapagamento.registro_evento_periodo\
     , folhapagamento.registro_evento    \
     , folhapagamento.ultimo_registro_evento \
left join folhapagamento.registro_evento_parcela\
       on ultimo_registro_evento.cod_registro = registro_evento_parcela.cod_registro\
      and ultimo_registro_evento.cod_evento = registro_evento_parcela.cod_evento\
      and ultimo_registro_evento.timestamp = registro_evento_parcela.timestamp\
     , folhapagamento.evento\
 where registro_evento_periodo.cod_registro = registro_evento.cod_registro\
   and registro_evento.cod_registro = ultimo_registro_evento.cod_registro\
   and registro_evento.cod_evento = evento.cod_evento\
   and registro_evento_periodo.cod_periodo_movimentacao = ?\
   and registro_evento_periodo.cod_contrato = ?"   
}  
if(params["inCodConfiguracao"] == 2){
        this.queryText  = "select evento.codigo \
             , evento.descricao \
             , evento.natureza \
             , evento.tipo     \
             , false as proporcional     \
             , CASE registro_evento_ferias.desdobramento \
               when 'A' then 'Abono' \
               when 'F' then 'Férias' \
               when 'D' then 'Adiantamento' \
               end as desdobramento \
