            <xml-property name="queryText"><![CDATA[SELECT registro              
     , cod_contrato          
     , numcgm                
     , nom_cgm               
     , to_char(dt_inicio_contagem, 'dd/mm/yyyy')    as dt_inicio_contagem   
     , to_char(dt_inicial_aquisitivo, 'dd/mm/yyyy') as dt_inicial_aquisitivo
     , to_char(dt_final_aquisitivo, 'dd/mm/yyyy')   as dt_final_aquisitivo  
     , to_char(dt_inicial_gozo, 'dd/mm/yyyy')       as dt_inicial_gozo       
     , to_char(dt_final_gozo, 'dd/mm/yyyy')         as dt_final_gozo        
     , faltas                
     , dias_ferias           
     , dias_abono    
     , dias
     , abono        
     , mes_pagamento         
     , folha                 
     , pagar_13              
     , lotacao               
     , local                 
     , regime                
     , funcao                
     , dt_posse              
     , dt_nomeacao  
     , cod_lotacao
     , cod_regime   
     , cod_local]]></xml-property>
