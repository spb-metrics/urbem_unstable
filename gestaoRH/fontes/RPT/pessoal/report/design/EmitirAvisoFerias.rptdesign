            <xml-property name="queryText"><![CDATA[SELECT sw_cgm.nom_cgm
     , sw_cgm.fone_residencial
     , sw_cgm.fone_comercial
     , sw_cgm.e_mail
     , sw_cgm.tipo_logradouro||' '||sw_cgm.logradouro as logradouro
     , sw_cgm.numero
     , sw_cgm.bairro
     , CASE WHEN logotipo IS NULL THEN ( select '../../../../../../gestaoAdministrativa/fontes/PHP/framework/temas/padrao/imagens/'||valor as logotipo from administracao.configuracao where parametro='logotipo' AND exercicio = ?)
         ELSE '../../../../../../gestaoFinanceira/fontes/PHP/orcamento/anexos/'||entidade_logotipo.logotipo END as logotipo 
     , substr(cnpj,1,2)||'.'||substr(cnpj,3,3)||'.'||substr(cnpj,6,3)||'/'||substr(cnpj,9,4)||'-'||substr(cnpj,13,2) as cnpj
     , sw_municipio.nom_municipio
     , sw_uf.sigla_uf
     , cep
  FROM orcamento.entidade
     LEFT JOIN orcamento.entidade_logotipo
       ON entidade.cod_entidade = entidade_logotipo.cod_entidade
     , sw_cgm
LEFT JOIN sw_cgm_pessoa_juridica
       ON sw_cgm.numcgm = sw_cgm_pessoa_juridica.numcgm
     , sw_municipio
     , sw_uf
 WHERE entidade.numcgm = sw_cgm.numcgm
   AND sw_cgm.cod_uf = sw_municipio.cod_uf
   AND sw_cgm.cod_municipio = sw_municipio.cod_municipio
   AND sw_cgm.cod_uf = sw_uf.cod_uf]]></xml-property>
