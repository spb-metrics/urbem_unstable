                <structure>
                    <property name="position">1</property>
                    <property name="name">cod_risco_fiscal</property>
                    <property name="nativeName">cod_risco_fiscal</property>
                    <property name="dataType">integer</property>
                </structure>
                <structure>
                    <property name="position">2</property>
                    <property name="name">desc_risco_fiscal</property>
                    <property name="nativeName">desc_risco_fiscal</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">3</property>
                    <property name="name">valor_risco_fiscal</property>
                    <property name="nativeName">valor_risco_fiscal</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">4</property>
                    <property name="name">desc_providencia</property>
                    <property name="nativeName">desc_providencia</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">5</property>
                    <property name="name">valor_providencia</property>
                    <property name="nativeName">valor_providencia</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">6</property>
                    <property name="name">total_risco_fiscal</property>
                    <property name="nativeName">total_risco_fiscal</property>
                    <property name="dataType">decimal</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[    SELECT rf.cod_risco_fiscal, 
           rf.descricao AS desc_risco_fiscal, 
           rf.valor AS valor_risco_fiscal,
           pf.descricao AS desc_providencia, 
           pf.valor AS valor_providencia,
           (SELECT SUM(valor)
              FROM ldo.risco_fiscal
           ) AS total_risco_fiscal
      FROM ldo.risco_fiscal rf
INNER JOIN ldo.providencia_fiscal pf 
        ON rf.cod_risco_fiscal = pf.cod_risco_fiscal
     WHERE rf.ano = 2010
  GROUP BY rf.cod_risco_fiscal,
           rf.descricao,
           rf.valor,
           pf.descricao,
           pf.valor]]></xml-property>
        </oda-data-set>
