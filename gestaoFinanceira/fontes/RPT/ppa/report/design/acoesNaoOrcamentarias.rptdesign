        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="dsTotais" id="1878">
            <structure name="cachedMetaData"/>
            <method name="beforeOpen"><![CDATA[if (params['cod_tipo_acao'] > 0) {
