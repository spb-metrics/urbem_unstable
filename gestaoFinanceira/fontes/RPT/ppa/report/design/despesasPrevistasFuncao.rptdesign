    <body>
        <table id="1305">
            <property name="fontFamily">sans-serif</property>
            <property name="fontSize">10pt</property>
            <property name="fontWeight">normal</property>
            <property name="textAlign">left</property>
            <property name="pageBreakAfter">auto</property>
            <property name="height">0.9489795918367347in</property>
            <property name="width">11.020408163265307in</property>
            <property name="dataSet">dsDespesas</property>
            <list-property name="boundDataColumns">
                <structure>
                    <property name="name">cod_funcao</property>
                    <expression name="expression">dataSetRow["cod_funcao"]</expression>
                    <property name="dataType">integer</property>
                </structure>
                <structure>
                    <property name="name">descricao</property>
                    <expression name="expression">dataSetRow["descricao"]</expression>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="name">ano</property>
                    <expression name="expression">dataSetRow["ano"]</expression>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="name">sum_valor_corrente</property>
                    <expression name="expression">dataSetRow["sum_valor_corrente"]</expression>
                    <property name="dataType">float</property>
                </structure>
                <structure>
                    <property name="name">sum_valor_capital</property>
                    <expression name="expression">dataSetRow["sum_valor_capital"]</expression>
                    <property name="dataType">float</property>
                </structure>
                <structure>
                    <property name="name">despesas_ano</property>
                    <property name="displayName">despesas_ano</property>
                    <expression name="expression">'DESPESAS PREVISTAS PARA ' + (params["ano_inicio"].value + (Number(dataSetRow["ano"])-1)) + ' POR FUNÇÃO'</expression>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="name">funcao_descricao</property>
                    <property name="displayName">funcao_descricao</property>
                    <expression name="expression">dataSetRow["cod_funcao"]+' - '+dataSetRow["descricao"]</expression>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="name">total_funcao</property>
                    <property name="displayName">total_funcao</property>
                    <expression name="expression">dataSetRow["sum_valor_corrente"]+dataSetRow["sum_valor_capital"]</expression>
                    <property name="dataType">float</property>
