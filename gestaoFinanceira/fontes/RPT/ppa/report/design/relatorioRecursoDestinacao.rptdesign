                </structure>
                <structure>
                    <property name="position">4</property>
                    <property name="name">valor</property>
                    <property name="nativeName">valor</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">5</property>
                    <property name="name">cod_programa</property>
                    <property name="nativeName">cod_programa</property>
                    <property name="dataType">integer</property>
                </structure>
                <structure>
                    <property name="position">6</property>
                    <property name="name">identificacao</property>
                    <property name="nativeName">identificacao</property>
                    <property name="dataType">string</property>
                </structure>
            </list-property>
            <property name="queryText">SELECT r.nom_recurso
     , ar.cod_recurso
     , CASE
       WHEN edr.descricao IS NULL
       THEN rdi.finalidade
       ELSE edr.descricao
       END AS descricao
     , SUM(valor) AS valor
     , p.cod_programa
     , pd.identificacao
      FROM ppa.acao_recurso AS ar
INNER JOIN ppa.acao AS a
        ON a.ativo = 't'
       AND a.cod_acao = ar.cod_acao
       AND a.ultimo_timestamp_acao_dados = ar.timestamp_acao_dados
INNER JOIN ppa.programa as p
        ON a.cod_programa = p.cod_programa
INNER JOIN ppa.programa_dados as pd
        ON p.cod_programa = pd.cod_programa
       AND p.ultimo_timestamp_programa_dados = pd.timestamp_programa_dados
INNER JOIN orcamento.recurso AS r
        ON ar.cod_recurso = r.cod_recurso
       AND ar.exercicio_recurso = r.exercicio
 LEFT JOIN orcamento.recurso_destinacao AS rd
        ON r.cod_recurso = rd.cod_recurso
       AND r.exercicio = rd.exercicio
 LEFT JOIN orcamento.especificacao_destinacao_recurso AS edr
        ON rd.cod_especificacao = edr.cod_especificacao
       AND rd.exercicio = edr.exercicio
 LEFT JOIN orcamento.recurso_direto AS rdi
        ON r.cod_recurso = rdi.cod_recurso
       AND r.exercicio = rdi.exercicio
     WHERE p.cod_ppa = ?
           AND CASE
               WHEN ? IS NULL
                   THEN true
                   ELSE p.num_programa = ?
               END
           AND CASE
               WHEN ? IS NULL
                   THEN true
                   ELSE r.cod_recurso = ?
               END 
           AND p.ativo = 't' 
           AND ar.exercicio_recurso = ?
  GROUP BY r.nom_recurso
         , p.cod_programa
         , edr.descricao
         , rdi.finalidade
         , ar.cod_recurso
         , pd.identificacao</property>
