	 	 , prr.cod_recurso as cod_recurso
	 	 , prr.cod_conta 
	 	 , prr.cod_entidade 
	 	 , prr.cod_receita_dados 
	 	 , prr.exercicio_recurso 
	 	 , prrv.ano 
	 	 , prrv.valor 
	 	 , CASE prrv.ano
			WHEN '0' THEN '5'
			ELSE prrv.ano
		   END as ordem_ano
	 	 , CASE prrv.ano
	 	 	WHEN 0 THEN 'Total'
	 	 	ELSE 'Ano '||prrv.ano
	 	   END as ano_valor
	 	 , 'R$ '||to_real(prrv.valor) as valor_real
	  	 , CASE ppa.destinacao_recurso
		       WHEN true THEN ordes.nom_recurso
		       ELSE ord.nom_recurso
		   END as nom_recurso
      FROM ppa.ppa_receita_recurso prr
INNER JOIN ppa.ppa_receita_recurso_valor prrv
	ON prr.cod_receita = prrv.cod_receita
       AND prr.cod_recurso = prrv.cod_recurso
       AND prr.cod_ppa = prrv.cod_ppa
       AND prr.exercicio = prrv.exercicio
       AND prr.cod_conta = prrv.cod_conta
       AND prr.cod_entidade = prrv.cod_entidade
       AND prr.cod_receita_dados = prrv.cod_receita_dados
INNER JOIN ppa.ppa as ppa
		ON ppa.cod_ppa = prr.cod_ppa
 LEFT JOIN ( SELECT ord.exercicio as exercicio 
		 	  	  , ore.cod_recurso as cod_recurso 
		 		  , ore.nom_recurso as nom_recurso 
	      	   FROM orcamento.recurso_destinacao as ord 
		 INNER JOIN orcamento.recurso as ore 
				 ON ord.cod_recurso = ore.cod_recurso 
	       		AND ord.exercicio = ore.exercicio ) as ordes 
		ON ordes.exercicio = prr.exercicio_recurso 
       AND ordes.cod_recurso = prr.cod_recurso 
 LEFT JOIN ( SELECT ord.cod_recurso as cod_recurso 
		 		  , ord.nom_recurso as nom_recurso
		 		  , ord.exercicio as exercicio  
	      	   FROM orcamento.recurso_direto as ord
         INNER JOIN orcamento.recurso as ore 
	        	 ON ord.cod_recurso = ore.cod_recurso 
	       		AND ord.exercicio = ore.exercicio) as ord 
		ON ord.exercicio = prr.exercicio_recurso 
       AND ord.cod_recurso = prr.cod_recurso 
     WHERE prr.cod_receita = ?            
       AND prr.cod_ppa = ? 
       AND prr.exercicio = ? 
       AND prr.cod_conta = ? 
       AND prr.cod_entidade = ? 
       AND prr.cod_receita_dados = ? 
  ORDER BY prr.cod_recurso, ordem_ano</property>
