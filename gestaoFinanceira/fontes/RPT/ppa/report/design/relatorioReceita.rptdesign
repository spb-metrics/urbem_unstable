            <property name="queryText">SELECT 'R$ ' || to_real(COALESCE(round(sum(receitas.valor_num), 2) , 0.00)) as valor_total_receita 
  FROM (
         SELECT DISTINCT 
	   pr.cod_receita as cod_receita
	 , ocr.descricao as descricao 
	 , ocr.exercicio as exercicio 
	 , p.cod_ppa as cod_ppa 
	 , pr.cod_receita ||' / '|| ocr.descricao as cod_desc 
	 , 'R$ ' || to_real(round(pr.valor_total, 2)) as valor_total 
	 , pr.valor_total as valor_num 
	 , pr.ativo as ativo 
	 , pn.cod_norma as cod_norma 
	 , pr.cod_conta as cod_conta 
	 , prr.cod_entidade as cod_entidade 
     , MAX(prr.cod_receita_dados) as cod_receita_dados 
      FROM ppa.ppa_receita AS pr 
INNER JOIN orcamento.conta_receita AS ocr 
		ON pr.cod_conta = ocr.cod_conta 
       AND pr.exercicio = ocr.exercicio 
INNER JOIN ppa.ppa AS p 
        ON p.cod_ppa = pr.cod_ppa 
INNER JOIN ppa.ppa_receita_dados AS prd 
		ON prd.cod_receita = pr.cod_receita 
       AND prd.cod_ppa = pr.cod_ppa 
       AND prd.exercicio = pr.exercicio 
       AND prd.cod_entidade = pr.cod_entidade 
       AND prd.cod_conta = pr.cod_conta 
 LEFT JOIN ppa.ppa_receita_recurso AS prr                              
  		ON prr.cod_receita = prd.cod_receita 
       AND prr.cod_ppa = prd.cod_ppa
       AND prr.exercicio = prd.exercicio 
       AND prr.cod_conta = prd.cod_conta 
       AND prr.cod_entidade = prd.cod_entidade 
       AND prr.cod_receita_dados = prd.cod_receita_dados 
 LEFT JOIN orcamento.recurso AS ore 
		ON ore.exercicio = prr.exercicio_recurso 
       AND ore.cod_recurso = prr.cod_recurso 
 LEFT JOIN ppa.ppa_norma AS pn                                   
  		ON pn.cod_ppa = p.cod_ppa 
     WHERE p.cod_ppa = ? 
       AND ( CASE ?
			 	WHEN 'maior' THEN pr.cod_conta >= ?
				WHEN 'menor' THEN pr.cod_conta &lt;= ?
				WHEN 'compreendido' THEN pr.cod_conta BETWEEN ? AND ?
				WHEN 'todos' THEN TRUE
	      	  END )
       AND pr.ativo = 't'
       AND ( CASE ?
       			WHEN 'todos' THEN TRUE
       			ELSE  prr.cod_recurso = ?
       		  END )
  GROUP BY pr.cod_receita
	 	 , ocr.descricao 
	 	 , ocr.exercicio 
	 	 , p.cod_ppa 
	 	 , pr.valor_total 
	 	 , pr.ativo 
	 	 , pn.cod_norma 
	 	 , pr.cod_conta 
	 	 , prr.cod_entidade 
       ) as receitas </property>
