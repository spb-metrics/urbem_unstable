this.queryText = this.queryText + "	             WHERE acao.cod_programa = programa.cod_programa                                 \n";
this.queryText = this.queryText + "                AND acao_dados.cod_tipo < 4)                                                  \n";
this.queryText = this.queryText + "  ORDER BY programa_dados.cod_tipo_programa                                                   \n";
