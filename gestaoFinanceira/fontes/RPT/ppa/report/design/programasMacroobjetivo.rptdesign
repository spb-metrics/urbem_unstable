this.queryText = this.queryText + "INNER JOIN ppa.programa_setorial                                                              \n";
this.queryText = this.queryText + "        ON programa_setorial.cod_macro = macro_objetivo.cod_macro                             \n";
this.queryText = this.queryText + "INNER JOIN ppa.programa                                                                       \n";
this.queryText = this.queryText + "        ON programa.cod_setorial = programa_setorial.cod_setorial                             \n";
this.queryText = this.queryText + "INNER JOIN ppa.programa_dados                                                                 \n";
this.queryText = this.queryText + "    	   ON programa_dados.cod_programa = programa.cod_programa                                \n";
this.queryText = this.queryText + "       AND programa_dados.timestamp_programa_dados = programa.ultimo_timestamp_programa_dados \n";
this.queryText = this.queryText + "INNER JOIN ppa.tipo_programa                                                                  \n";
this.queryText = this.queryText + "    	   ON tipo_programa.cod_tipo_programa = programa_dados.cod_tipo_programa                 \n";
this.queryText = this.queryText + "INNER JOIN orcamento.unidade                                                                  \n";
this.queryText = this.queryText + "        ON unidade.exercicio = programa_dados.exercicio_unidade                               \n";
this.queryText = this.queryText + "       AND unidade.num_unidade = programa_dados.num_unidade                                   \n";
