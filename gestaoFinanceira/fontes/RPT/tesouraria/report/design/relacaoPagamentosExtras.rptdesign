            <property name="queryText">    select
        to_char(pagamentos.timestamp, 'dd/mm/yyyy') as dt_emissao
        ,empenho.cod_entidade
        ,conta_despesa.cod_estrutural as estrutural_despesa
        ,cgm.nom_cgm
        ,cgm.cpf_cnpj
        ,pagamentos.cod_estrutural as estrutural_credito
        ,cast(pagamentos.cod_ordem as varchar) as cod_ordem
        ,pagamentos.nom_conta
        ,pagamentos.saldo_pagamento
    from 
        (
            select  nlp.cod_entidade                                                                                           
                   ,nlp.cod_nota                                                                                               
                   ,plnlp.cod_ordem   
                   ,nlp.observacao                                                                                         
                   ,plnlp.exercicio                                                                                            
                   ,plnlp.exercicio as exercicio_liquidacao                                                                      
                   ,pb.conta_corrente
                   ,pc.cod_estrutural
                   ,pc.nom_conta
                   ,nlp.timestamp
                   ,sum(coalesce(nlp.vl_pago ,0.00)) as vl_pago                                                            
                   ,sum(coalesce(nlpa.vl_anulado ,0.00)) as vl_anulado                                                         
                   ,( sum(nlp.vl_pago) - coalesce(nlpa.vl_anulado,0.00) ) as saldo_pagamento                      
            from  empenho.pagamento_liquidacao_nota_liquidacao_paga as plnlp                                                   
                 ,empenho.nota_liquidacao_paga as nlp                                                                          
                  left join (                                                                                       
                                  SELECT exercicio                                                                  
                                        ,cod_nota                                                                   
                                        ,cod_entidade                                                               
                                        ,timestamp                                                                  
                                        ,coalesce(sum(nlpa.vl_anulado),0.00) as vl_anulado                          
                                    FROM empenho.nota_liquidacao_paga_anulada as nlpa                               
                                  GROUP BY exercicio, cod_nota, cod_entidade, timestamp                             
                  ) as nlpa on (     nlp.exercicio    = nlpa.exercicio                                              
                               and nlp.cod_nota     = nlpa.cod_nota             
                               and nlp.cod_entidade = nlpa.cod_entidade         
                               and nlp.timestamp    = nlpa.timestamp            
                               )                                                   
            
                 ,empenho.nota_liquidacao_conta_pagadora as nlcp
                 ,contabilidade.plano_analitica as pa
                 ,contabilidade.plano_banco as pb
                 ,contabilidade.plano_conta as pc
            where     nlp.cod_entidade = plnlp.cod_entidade                                                                    
                  and nlp.cod_nota     = plnlp.cod_nota                                                                        
                  and nlp.exercicio    = plnlp.exercicio_liquidacao                                                            
                  and nlp.timestamp    = plnlp.timestamp        
            
                  and nlcp.cod_entidade         = plnlp.cod_entidade
                  and nlcp.cod_nota             = plnlp.cod_nota
                  and nlcp.timestamp            = plnlp.timestamp
                  and nlcp.exercicio_liquidacao = plnlp.exercicio                                                               
            
                  and nlcp.exercicio = pa.exercicio
                  and nlcp.cod_plano = pa.cod_plano
            
                  and pa.cod_plano = pb.cod_plano
                  and pa.exercicio = pb.exercicio
    
                  and pc.exercicio = pa.exercicio
                  and pc.cod_conta = pa.cod_conta
            
            group by  nlp.cod_entidade                                                                                         
                     ,nlp.cod_nota                                                                                             
                     ,nlp.exercicio                                                                                            
                     ,nlp.observacao
                     ,nlpa.vl_anulado                                                                                          
                     ,plnlp.cod_ordem                                                                                          
                     ,plnlp.exercicio                                                                                        
                     ,nlp.timestamp
                     ,pb.conta_corrente
                     ,pc.nom_conta
                     ,pc.cod_estrutural
        ) as pagamentos
        ,empenho.ordem_pagamento
        ,empenho.pagamento_liquidacao
        ,empenho.nota_liquidacao
        ,empenho.empenho
        ,empenho.pre_empenho
        ,empenho.pre_empenho_despesa
        ,orcamento.conta_despesa
        ,(
            select 
                case when
                    cgm_fisica.cpf is null
                then
                    COALESCE(cgm_juridica.cnpj,'')
                else
                    COALESCE(cgm_fisica.cpf, '')
                end as cpf_cnpj
                ,cgm.numcgm
                ,cgm.nom_cgm
            from
                sw_cgm as cgm
            left join
                sw_cgm_pessoa_fisica as cgm_fisica
            on
                cgm.numcgm = cgm_fisica.numcgm
            left join
                sw_cgm_pessoa_juridica as cgm_juridica
            on
                cgm.numcgm = cgm_juridica.numcgm
    
        )as cgm
    where
    
        pagamentos.exercicio = pagamento_liquidacao.exercicio
        and pagamentos.exercicio_liquidacao = pagamento_liquidacao.exercicio_liquidacao
        and pagamentos.cod_entidade = pagamento_liquidacao.cod_entidade
        and pagamentos.cod_nota = pagamento_liquidacao.cod_nota
        and pagamentos.cod_ordem = pagamento_liquidacao.cod_ordem
    
        and nota_liquidacao.exercicio = pagamentos.exercicio
        and nota_liquidacao.cod_entidade = pagamentos.cod_entidade
        and nota_liquidacao.cod_nota = pagamentos.cod_nota
    
        and empenho.exercicio = nota_liquidacao.exercicio_empenho
        and empenho.cod_empenho = nota_liquidacao.cod_empenho
    
        and empenho.exercicio = pre_empenho.exercicio
        and empenho.cod_pre_empenho = pre_empenho.cod_pre_empenho
    
        and pre_empenho.exercicio = pre_empenho_despesa.exercicio
        and pre_empenho.cod_pre_empenho = pre_empenho_despesa.cod_pre_empenho
    
        and pre_empenho_despesa.exercicio = conta_despesa.exercicio
        and pre_empenho_despesa.cod_conta = conta_despesa.cod_conta
    
        and pagamento_liquidacao.exercicio = ordem_pagamento.exercicio
        and pagamento_liquidacao.cod_entidade = ordem_pagamento.cod_entidade
        and pagamento_liquidacao.cod_ordem = ordem_pagamento.cod_ordem
    
        and cgm.numcgm = pre_empenho.cgm_beneficiario
        
--        and empenho.exercicio &lt; ?
--       and (pagamentos.exercicio_liquidacao = ?)
--        and empenho.cod_entidade = ?
--        and ordem_pagamento.dt_emissao between to_date( ?, 'dd/mm/yyyy') and to_date( ?, 'dd/mm/yyyy')

    union

    select
         to_char(transferencia.timestamp_transferencia,'dd/mm/yyyy') as dt_emissao
        ,transferencia.cod_entidade
        ,plano_debito.cod_estrutural as estrutural_debito
        ,cgm_credor.nom_cgm
        ,cgm_credor.cpf_cnpj
        ,plano_credito.cod_estrutural as estrutural_credito
        ,'' as cod_ordem
        ,plano_credito.nom_conta as descricao
        ,coalesce(transferencia.valor,0.00) as saldo_pagamento
    from
        tesouraria.transferencia
    left join
        (
           select
               case when
                   cgm_fisica.cpf is null
               then
                   COALESCE(cgm_juridica.cnpj,'')
               else
                   COALESCE(cgm_fisica.cpf, '')
               end as cpf_cnpj
               ,cgm.numcgm
               ,cgm.nom_cgm
               ,credor.cod_lote
               ,credor.cod_entidade
               ,credor.exercicio
               ,credor.tipo
           from
               sw_cgm as cgm
           left join
               sw_cgm_pessoa_fisica as cgm_fisica
           on
               cgm.numcgm = cgm_fisica.numcgm
           left join
               sw_cgm_pessoa_juridica as cgm_juridica
           on
               cgm.numcgm = cgm_juridica.numcgm
              ,tesouraria.transferencia_credor as credor
           where
              cgm.numcgm = credor.numcgm
    
       )as cgm_credor
    on
            cgm_credor.cod_lote     = transferencia.cod_lote
        and cgm_credor.cod_entidade = transferencia.cod_entidade
        and cgm_credor.exercicio    = transferencia.exercicio
        and cgm_credor.tipo         = transferencia.tipo
       ,(
            select
                 plano_conta.cod_estrutural
                ,plano_conta.cod_conta
                ,plano_analitica.cod_plano
                ,plano_analitica.exercicio
            from
                 contabilidade.plano_analitica
                ,contabilidade.plano_conta
            where
                    plano_analitica.exercicio = plano_conta.exercicio
                and plano_analitica.cod_conta = plano_conta.cod_conta
        ) as plano_debito
        ,(
             select
                 plano_conta.cod_estrutural
                ,plano_conta.nom_conta
                ,plano_analitica.cod_plano
                ,plano_analitica.exercicio
            from
                 contabilidade.plano_analitica
                ,contabilidade.plano_conta
            where
                    plano_analitica.exercicio = plano_conta.exercicio
                and plano_analitica.cod_conta = plano_conta.cod_conta
       ) as plano_credito
    
    where
            transferencia.cod_plano_debito = plano_debito.cod_plano
        and transferencia.exercicio        = plano_debito.exercicio
    
        and transferencia.cod_plano_credito = plano_credito.cod_plano
        and transferencia.exercicio     = plano_credito.exercicio

--        and transferencia.exercicio = ?
--        and (transferencia.cod_entidade = ?)
--        and transferencia.timestamp_transferencia between to_date( ?, 'dd/mm/yyyy') and to_date( ?, 'dd/mm/yyyy')
limit 10</property>
