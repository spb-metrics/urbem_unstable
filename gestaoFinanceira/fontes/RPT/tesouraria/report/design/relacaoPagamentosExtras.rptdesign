            <property name="dataSource">Postgres</property>
            <method name="beforeOpen"><![CDATA[this.queryText =  " SELECT                                                                                                 \n"; 
this.queryText += "     to_char(pagamentos.timestamp, 'dd/mm/yyyy') as dt_emissao                                          \n";
this.queryText += "     ,empenho.cod_entidade                                                                              \n";
this.queryText += "     ,conta_despesa.cod_estrutural as estrutural_despesa                                                \n";
this.queryText += "     ,cgm.cpf_cnpj ||' '||cgm.nom_cgm as cpf_cnpj                                                       \n";
this.queryText += "     ,pagamentos.cod_estrutural as estrutural_credito                                                   \n";
this.queryText += "     ,cast(pagamentos.cod_ordem as varchar) as cod_ordem                                                \n";
this.queryText += "     ,pagamentos.nom_conta                                                                              \n";
this.queryText += "     ,pagamentos.saldo_pagamento                                                                        \n";
this.queryText += " from                                                                                                   \n";
this.queryText += "     (                                                                                                  \n";
this.queryText += "         select  nlp.cod_entidade                                                                       \n";                   
this.queryText += "                ,nlp.cod_nota                                                                           \n";                   
this.queryText += "                ,plnlp.cod_ordem                                                                        \n";
this.queryText += "                ,nlp.observacao                                                                         \n";               
this.queryText += "                ,plnlp.exercicio                                                                        \n";                   
this.queryText += "                ,plnlp.exercicio as exercicio_liquidacao                                                \n";                     
this.queryText += "                ,pb.conta_corrente                                                                      \n";
this.queryText += "                ,pc.cod_estrutural                                                                      \n";
this.queryText += "                ,pc.nom_conta                                                                           \n";
this.queryText += "                ,nlp.timestamp                                                                          \n";
this.queryText += "                ,sum(coalesce(nlp.vl_pago ,0.00)) as vl_pago                                            \n";               
this.queryText += "                ,sum(coalesce(nlpa.vl_anulado ,0.00)) as vl_anulado                                     \n";                   
this.queryText += "                ,( sum(nlp.vl_pago) - coalesce(nlpa.vl_anulado,0.00) ) as saldo_pagamento               \n";      
this.queryText += "         from  empenho.pagamento_liquidacao_nota_liquidacao_paga as plnlp                               \n";                   
this.queryText += "              ,empenho.nota_liquidacao_paga as nlp                                                      \n";                   
this.queryText += "               left join (                                                                              \n";        
this.queryText += "                               SELECT exercicio                                                         \n";        
this.queryText += "                                     ,cod_nota                                                          \n";        
this.queryText += "                                     ,cod_entidade                                                      \n";        
this.queryText += "                                     ,timestamp                                                         \n";        
this.queryText += "                                     ,coalesce(sum(nlpa.vl_anulado),0.00) as vl_anulado                 \n";        
this.queryText += "                                 FROM empenho.nota_liquidacao_paga_anulada as nlpa                      \n";        
this.queryText += "                               GROUP BY exercicio, cod_nota, cod_entidade, timestamp                    \n";        
this.queryText += "               ) as nlpa on (     nlp.exercicio    = nlpa.exercicio                                     \n";        
this.queryText += "                            and nlp.cod_nota     = nlpa.cod_nota                                        \n";
this.queryText += "                            and nlp.cod_entidade = nlpa.cod_entidade                                    \n";
this.queryText += "                            and nlp.timestamp    = nlpa.timestamp                                       \n";
this.queryText += "                            )                                                                           \n";
this.queryText += "                                                                                                        \n";
this.queryText += "              ,empenho.nota_liquidacao_conta_pagadora as nlcp                                           \n";
this.queryText += "              ,contabilidade.plano_analitica as pa                                                      \n";
this.queryText += "              ,contabilidade.plano_banco as pb                                                          \n";
this.queryText += "              ,contabilidade.plano_conta as pc                                                          \n";
this.queryText += "         where     nlp.cod_entidade = plnlp.cod_entidade                                                \n";                   
this.queryText += "               and nlp.cod_nota     = plnlp.cod_nota                                                    \n";                   
this.queryText += "               and nlp.exercicio    = plnlp.exercicio_liquidacao                                        \n";                   
this.queryText += "               and nlp.timestamp    = plnlp.timestamp                                                   \n";
this.queryText += "                                                                                                        \n";
this.queryText += "               and nlcp.cod_entidade         = plnlp.cod_entidade                                       \n";
this.queryText += "               and nlcp.cod_nota             = plnlp.cod_nota                                           \n";
this.queryText += "               and nlcp.timestamp            = plnlp.timestamp                                          \n";
this.queryText += "               and nlcp.exercicio_liquidacao = plnlp.exercicio                                          \n";                    
this.queryText += "                                                                                                        \n";
this.queryText += "               and nlcp.exercicio = pa.exercicio                                                        \n";
this.queryText += "               and nlcp.cod_plano = pa.cod_plano                                                        \n";
this.queryText += "                                                                                                        \n";
this.queryText += "               and pa.cod_plano = pb.cod_plano                                                          \n";
this.queryText += "               and pa.exercicio = pb.exercicio                                                          \n";
this.queryText += "                                                                                                        \n";
this.queryText += "               and pc.exercicio = pa.exercicio                                                          \n";
this.queryText += "               and pc.cod_conta = pa.cod_conta                                                          \n";
this.queryText += "                                                                                                        \n";
this.queryText += "         group by  nlp.cod_entidade                                                                     \n";                   
this.queryText += "                  ,nlp.cod_nota                                                                         \n";                   
this.queryText += "                  ,nlp.exercicio                                                                        \n";                   
this.queryText += "                  ,nlp.observacao                                                                       \n";
this.queryText += "                  ,nlpa.vl_anulado                                                                      \n";                   
this.queryText += "                  ,plnlp.cod_ordem                                                                      \n";                   
this.queryText += "                  ,plnlp.exercicio                                                                      \n";                 
this.queryText += "                  ,nlp.timestamp                                                                        \n";
this.queryText += "                  ,pb.conta_corrente                                                                    \n";
this.queryText += "                  ,pc.nom_conta                                                                         \n";
this.queryText += "                  ,pc.cod_estrutural                                                                    \n";
this.queryText += "     ) as pagamentos                                                                                    \n";
this.queryText += "     ,empenho.ordem_pagamento                                                                           \n";
this.queryText += "     ,empenho.pagamento_liquidacao                                                                      \n";
this.queryText += "     ,empenho.nota_liquidacao                                                                           \n";
this.queryText += "     ,empenho.empenho                                                                                   \n";
this.queryText += "     ,empenho.pre_empenho                                                                               \n";
this.queryText += "     ,empenho.pre_empenho_despesa                                                                       \n";
this.queryText += "     ,orcamento.conta_despesa                                                                           \n";
this.queryText += "     ,(                                                                                                 \n";
this.queryText += "         select                                                                                         \n";
this.queryText += "             case when                                                                                  \n";
this.queryText += "                 cgm_fisica.cpf is null                                                                 \n";
this.queryText += "             then                                                                                       \n";
this.queryText += "                 COALESCE(cgm_juridica.cnpj,'')                                                         \n";
this.queryText += "             else                                                                                       \n";
this.queryText += "                 COALESCE(cgm_fisica.cpf, '')                                                           \n";
this.queryText += "             end as cpf_cnpj                                                                            \n";
this.queryText += "             ,cgm.numcgm                                                                                \n";
this.queryText += "             ,cgm.nom_cgm                                                                               \n";
this.queryText += "         from                                                                                           \n";
this.queryText += "             sw_cgm as cgm                                                                              \n";
this.queryText += "         left join                                                                                      \n";
this.queryText += "             sw_cgm_pessoa_fisica as cgm_fisica                                                         \n";
this.queryText += "         on                                                                                             \n";
this.queryText += "             cgm.numcgm = cgm_fisica.numcgm                                                             \n";
this.queryText += "         left join                                                                                      \n";
this.queryText += "             sw_cgm_pessoa_juridica as cgm_juridica                                                     \n";
this.queryText += "         on                                                                                             \n";
this.queryText += "             cgm.numcgm = cgm_juridica.numcgm                                                           \n";
this.queryText += "                                                                                                        \n";
this.queryText += "     )as cgm                                                                                            \n";
this.queryText += " where                                                                                                  \n";
this.queryText += "         empenho.exercicio < '"+params["exercicio"]+"'                                                                     \n";
this.queryText += "     and pagamentos.exercicio_liquidacao = '"+params["exercicio"]+"'                                                       \n";
this.queryText += "     and empenho.cod_entidade in ("+params["entidade"]+")                                                              \n";
this.queryText += " --    and ordem_pagamento.dt_emissao between to_date('"+params["initial_date"]+"','mm-dd-yyyy') and to_date('"+params["final_date"]+"','mm-dd-yyyy') \n";
this.queryText += "                                                                                                        \n"; 
this.queryText += "     and pagamentos.exercicio = pagamento_liquidacao.exercicio                                          \n";
this.queryText += "     and pagamentos.exercicio_liquidacao = pagamento_liquidacao.exercicio_liquidacao                    \n";
this.queryText += "     and pagamentos.cod_entidade = pagamento_liquidacao.cod_entidade                                    \n";
this.queryText += "     and pagamentos.cod_nota = pagamento_liquidacao.cod_nota                                            \n";
this.queryText += "     and pagamentos.cod_ordem = pagamento_liquidacao.cod_ordem                                          \n";
this.queryText += "                                                                                                        \n";
this.queryText += "     and nota_liquidacao.exercicio = pagamentos.exercicio                                               \n";
this.queryText += "     and nota_liquidacao.cod_entidade = pagamentos.cod_entidade                                         \n";
this.queryText += "     and nota_liquidacao.cod_nota = pagamentos.cod_nota                                                 \n";
this.queryText += "                                                                                                        \n";
this.queryText += "     and empenho.exercicio = nota_liquidacao.exercicio_empenho                                          \n";
this.queryText += "     and empenho.cod_empenho = nota_liquidacao.cod_empenho                                              \n";
this.queryText += "                                                                                                        \n";
this.queryText += "     and empenho.exercicio = pre_empenho.exercicio                                                      \n";
this.queryText += "     and empenho.cod_pre_empenho = pre_empenho.cod_pre_empenho                                          \n";
this.queryText += "                                                                                                        \n";
this.queryText += "     and pre_empenho.exercicio = pre_empenho_despesa.exercicio                                          \n";
this.queryText += "     and pre_empenho.cod_pre_empenho = pre_empenho_despesa.cod_pre_empenho                              \n";
this.queryText += "                                                                                                        \n";
this.queryText += "     and pre_empenho_despesa.exercicio = conta_despesa.exercicio                                        \n";
this.queryText += "     and pre_empenho_despesa.cod_conta = conta_despesa.cod_conta                                        \n";
this.queryText += "                                                                                                        \n";
this.queryText += "     and pagamento_liquidacao.exercicio = ordem_pagamento.exercicio                                     \n";
this.queryText += "     and pagamento_liquidacao.cod_entidade = ordem_pagamento.cod_entidade                               \n";
this.queryText += "     and pagamento_liquidacao.cod_ordem = ordem_pagamento.cod_ordem                                     \n";
this.queryText += "                                                                                                        \n";
this.queryText += "     and cgm.numcgm = pre_empenho.cgm_beneficiario                                                      \n";
this.queryText += "                                                                                                        \n";
this.queryText += " union                                                                                                  \n";
this.queryText += "                                                                                                        \n";
this.queryText += " select                                                                                                 \n";
this.queryText += "      to_char(transferencia.timestamp_transferencia,'dd/mm/yyyy') as dt_emissao                         \n";
this.queryText += "     ,transferencia.cod_entidade                                                                        \n";
this.queryText += "     ,plano_debito.cod_estrutural as estrutural_debito                                                  \n";

this.queryText += "        ,case when                                          \n";
this.queryText += "            cgm_credor.cpf_cnpj is null                     \n";
this.queryText += "        then                                                \n";
this.queryText += "            plano_debito.nom_conta                          \n";
this.queryText += "        else                                                \n";
this.queryText += "            cgm_credor.cpf_cnpj ||' '||cgm_credor.nom_cgm   \n";
this.queryText += "        end as cpf_cnpj                                     \n";

this.queryText += "     ,plano_credito.cod_estrutural as estrutural_credito                                                \n";
this.queryText += "     ,'' as cod_ordem                                                                                   \n";
this.queryText += "     ,plano_credito.nom_conta as descricao                                                              \n";
this.queryText += "     ,coalesce(transferencia.valor,0.00) as saldo_pagamento                                             \n";
this.queryText += " from                                                                                                   \n";
this.queryText += "     tesouraria.transferencia                                                                           \n";
this.queryText += " left join                                                                                              \n";
this.queryText += "     (                                                                                                  \n";
this.queryText += "        select                                                                                          \n";
this.queryText += "            case when                                                                                   \n";
this.queryText += "                cgm_fisica.cpf is null                                                                  \n";
this.queryText += "            then                                                                                        \n";
this.queryText += "                COALESCE(cgm_juridica.cnpj,'')                                                          \n";
this.queryText += "            else                                                                                        \n";
this.queryText += "                COALESCE(cgm_fisica.cpf, '')                                                            \n";
this.queryText += "            end as cpf_cnpj                                                                             \n";
this.queryText += "            ,cgm.numcgm                                                                                 \n";
this.queryText += "            ,cgm.nom_cgm                                                                                \n";
this.queryText += "            ,credor.cod_lote                                                                            \n";
this.queryText += "            ,credor.cod_entidade                                                                        \n";
this.queryText += "            ,credor.exercicio                                                                           \n";
this.queryText += "            ,credor.tipo                                                                                \n";
this.queryText += "        from                                                                                            \n";
this.queryText += "            sw_cgm as cgm                                                                               \n";
this.queryText += "        left join                                                                                       \n";
this.queryText += "            sw_cgm_pessoa_fisica as cgm_fisica                                                          \n";
this.queryText += "        on                                                                                              \n";
this.queryText += "            cgm.numcgm = cgm_fisica.numcgm                                                              \n";
this.queryText += "        left join                                                                                       \n";
this.queryText += "            sw_cgm_pessoa_juridica as cgm_juridica                                                      \n";
this.queryText += "        on                                                                                              \n";
this.queryText += "            cgm.numcgm = cgm_juridica.numcgm                                                            \n";
this.queryText += "           ,tesouraria.transferencia_credor as credor                                                   \n";
this.queryText += "        where                                                                                           \n";
this.queryText += "           cgm.numcgm = credor.numcgm                                                                   \n";
this.queryText += "                                                                                                        \n";
this.queryText += "    )as cgm_credor                                                                                      \n";
this.queryText += " on                                                                                                     \n";
this.queryText += "         cgm_credor.cod_lote     = transferencia.cod_lote                                               \n";
this.queryText += "     and cgm_credor.cod_entidade = transferencia.cod_entidade                                           \n";
this.queryText += "     and cgm_credor.exercicio    = transferencia.exercicio                                              \n";
this.queryText += "     and cgm_credor.tipo         = transferencia.tipo                                                   \n";
this.queryText += "    ,(                                                                                                  \n";
this.queryText += "         select                                                                                         \n";
this.queryText += "              plano_conta.cod_estrutural                                                                \n";
this.queryText += "             ,plano_conta.cod_conta                                                                     \n";
this.queryText += "             ,plano_conta.nom_conta                                                                     \n";
this.queryText += "             ,plano_analitica.cod_plano                                                                 \n";
this.queryText += "             ,plano_analitica.exercicio                                                                 \n";
this.queryText += "         from                                                                                           \n";
this.queryText += "              contabilidade.plano_analitica                                                             \n";
this.queryText += "             ,contabilidade.plano_conta                                                                 \n";
this.queryText += "         where                                                                                          \n";
this.queryText += "                 plano_analitica.exercicio = plano_conta.exercicio                                      \n";
this.queryText += "             and plano_analitica.cod_conta = plano_conta.cod_conta                                      \n";
this.queryText += "     ) as plano_debito                                                                                  \n";
this.queryText += "     ,(                                                                                                 \n";
this.queryText += "          select                                                                                        \n";
this.queryText += "              plano_conta.cod_estrutural                                                                \n";
this.queryText += "             ,plano_conta.nom_conta                                                                     \n";
this.queryText += "             ,plano_analitica.cod_plano                                                                 \n";
this.queryText += "             ,plano_analitica.exercicio                                                                 \n";
this.queryText += "         from                                                                                           \n";
this.queryText += "              contabilidade.plano_analitica                                                             \n";
this.queryText += "             ,contabilidade.plano_conta                                                                 \n";
this.queryText += "         where                                                                                          \n";
this.queryText += "                 plano_analitica.exercicio = plano_conta.exercicio                                      \n";
this.queryText += "             and plano_analitica.cod_conta = plano_conta.cod_conta                                      \n";
this.queryText += "    ) as plano_credito                                                                                  \n";
this.queryText += "                                                                                                        \n";
this.queryText += " where                                                                                                  \n";
this.queryText += "         transferencia.cod_plano_debito = plano_debito.cod_plano                                        \n";
this.queryText += "     and transferencia.exercicio        = plano_debito.exercicio                                        \n";
this.queryText += "                                                                                                        \n";
this.queryText += "     and transferencia.cod_plano_credito = plano_credito.cod_plano                                      \n";
this.queryText += "     and transferencia.exercicio     = plano_credito.exercicio                                          \n";
this.queryText += "                                                                                                        \n";
this.queryText += "     and transferencia.exercicio = '"+params["exercicio"]+"'                                                               \n";
this.queryText += "     and transferencia.cod_entidade in ("+params["entidade"]+")                                                        \n";
this.queryText += "     and transferencia.timestamp_transferencia between to_date( '"+params["initial_date"]+"', 'dd/mm/yyyy') and to_date( '"+params["final_date"]+"', 'dd/mm/yyyy') \n";
this.queryText += "     and ((plano_credito.cod_estrutural not like '1.1.5.%' and plano_credito.cod_estrutural not like '1.1.1.1.1.%' and plano_credito.cod_estrutural not like '1.1.1.1.2.%') \n";
this.queryText += "     and ( plano_debito.cod_estrutural not like '1.1.5.%' and plano_debito.cod_estrutural not like '1.1.1.1.1.%' and plano_credito.cod_estrutural not like '1.1.1.1.2.%')) \n";

if( params["ccaixa"] != "" ) {
	this.queryText += " and transferencia.cod_plano_credito = "+params["ccaixa"]+" \n";
}

if( params["cdespesa"] != "" ) {	
	this.queryText += " and transferencia.cod_plano_debito = "+params["cdespesa"]+" \n";
}]]></method>
