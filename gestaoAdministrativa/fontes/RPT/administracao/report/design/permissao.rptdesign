            <xml-property name="queryText"><![CDATA[Select  u.numcgm,
		u.username,
		c.nom_cgm,
		p.cod_acao,
		f.cod_funcionalidade,
		m.cod_modulo,
		a.nom_acao,
		f.nom_funcionalidade,
		m.nom_modulo
From    administracao.permissao as p,
		administracao.acao as a,
		administracao.funcionalidade as f,
		administracao.modulo as m,
		administracao.usuario as u,
		sw_cgm as c
Where   p.numcgm = u.numcgm
And     u.numcgm = c.numcgm
And     p.cod_acao = a.cod_acao
And     a.cod_funcionalidade = f.cod_funcionalidade
And     f.cod_modulo = m.cod_modulo]]></xml-property>
